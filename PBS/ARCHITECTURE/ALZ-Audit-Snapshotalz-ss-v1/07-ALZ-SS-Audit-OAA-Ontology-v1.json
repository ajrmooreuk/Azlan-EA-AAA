  "@id": "baiv:uniregistry:ontology:alz-compliance-v1-0-0",
  "registryMetadata": {
    "entryType": "ontology",
    "entryId": "Entry-ONT-ALZ-001",
    "name": "ALZ Compliance Ontology",
    "version": "1.0.0",
    "status": "active",
    "domain": "cloud-governance",
    "tier": "tier2",
    "dateCreated": "2026-02-03T12:00:00Z",
    "lastModified": "2026-02-03T12:00:00Z",
    "createdBy": "oaa-v5.0.0",
    "description": "Ontology for Azure Landing Zone compliance management, mapping controls across MCSB v1, MCSB v2, NIST 800-53, UK NCSC 14 Principles, and ISO 27001 frameworks for Insurance Advisory sector",
    "tags": ["azure", "landing-zone", "compliance", "security", "insurance", "mcsb", "nist", "ncsc", "iso"]
  },
  "artifactDefinition": {
    "entities": [
      {
        "@id": "alz:ComplianceFramework",
        "@type": "DefinedTermSet",
        "name": "ComplianceFramework",
        "description": "A regulatory or industry compliance framework that defines security and governance controls for cloud environments",
        "schemaOrgAlignment": {
          "baseType": "DefinedTermSet",
          "rationale": "Compliance frameworks are collections of defined terms (controls)",
          "extensions": ["frameworkVersion", "regulatoryBody", "applicableSectors"]
        },
        "properties": [
          {"name": "frameworkId", "type": "Text", "required": true},
          {"name": "frameworkName", "type": "Text", "required": true},
          {"name": "frameworkVersion", "type": "Text", "required": true},
          {"name": "regulatoryBody", "type": "Organization", "required": false},
          {"name": "applicableSectors", "type": "Text", "cardinality": "0..*"},
          {"name": "policySetDefinitionId", "type": "URL", "required": false}
        ]
      },
      {
        "@id": "alz:ComplianceControl",
        "@type": "DefinedTerm",
        "name": "ComplianceControl",
        "description": "A specific security or governance requirement within a compliance framework that can be implemented and audited",
        "schemaOrgAlignment": {
          "baseType": "DefinedTerm",
          "rationale": "Controls are defined terms with specific implementation requirements",
          "extensions": ["controlId", "controlFamily", "implementationGuidance", "auditEvidence"]
        },
        "properties": [
          {"name": "controlId", "type": "Text", "required": true},
          {"name": "controlName", "type": "Text", "required": true},
          {"name": "controlFamily", "type": "Text", "required": true},
          {"name": "description", "type": "Text", "required": true},
          {"name": "implementationGuidance", "type": "Text", "required": false},
          {"name": "auditEvidence", "type": "Text", "cardinality": "0..*"}
        ]
      },
      {
        "@id": "alz:ControlMapping",
        "@type": "PropertyValue",
        "name": "ControlMapping",
        "description": "A mapping relationship between equivalent controls across different compliance frameworks enabling cross-framework compliance assessment",
        "schemaOrgAlignment": {
          "baseType": "PropertyValue",
          "rationale": "Mappings are property-value associations between controls",
          "extensions": ["mappingStrength", "mappingRationale"]
        },
        "properties": [
          {"name": "sourceControl", "type": "alz:ComplianceControl", "required": true},
          {"name": "targetControl", "type": "alz:ComplianceControl", "required": true},
          {"name": "mappingStrength", "type": "Text", "enum": ["exact", "partial", "related"], "required": true},
          {"name": "mappingRationale", "type": "Text", "required": false}
        ]
      },
      {
        "@id": "alz:AzureResource",
        "@type": "SoftwareApplication",
        "name": "AzureResource",
        "description": "An Azure cloud resource that can be configured and audited for compliance with security controls",
        "schemaOrgAlignment": {
          "baseType": "SoftwareApplication",
          "rationale": "Azure resources are software applications/services",
          "extensions": ["resourceType", "resourceProvider", "configurationProperties"]
        },
        "properties": [
          {"name": "resourceId", "type": "Text", "required": true},
          {"name": "resourceType", "type": "Text", "required": true},
          {"name": "resourceProvider", "type": "Text", "required": true},
          {"name": "subscriptionId", "type": "Text", "required": true},
          {"name": "resourceGroup", "type": "Text", "required": true},
          {"name": "location", "type": "Text", "required": true},
          {"name": "tags", "type": "PropertyValue", "cardinality": "0..*"}
        ]
      },
      {
        "@id": "alz:ControlImplementation",
        "@type": "Action",
        "name": "ControlImplementation",
        "description": "The specific Azure configuration or policy that implements a compliance control for a resource type",
        "schemaOrgAlignment": {
          "baseType": "Action",
          "rationale": "Implementations are actions taken to achieve compliance",
          "extensions": ["azurePolicy", "configurationSetting", "automationRunbook"]
        },
        "properties": [
          {"name": "implementationId", "type": "Text", "required": true},
          {"name": "implementationName", "type": "Text", "required": true},
          {"name": "azurePolicyId", "type": "URL", "required": false},
          {"name": "configurationProperty", "type": "Text", "required": false},
          {"name": "expectedValue", "type": "Text", "required": false},
          {"name": "remediationAction", "type": "Text", "required": false}
        ]
      },
      {
        "@id": "alz:ComplianceAssessment",
        "@type": "Dataset",
        "name": "ComplianceAssessment",
        "description": "A point-in-time evaluation of an Azure environment against compliance controls producing a compliance score",
        "schemaOrgAlignment": {
          "baseType": "Dataset",
          "rationale": "Assessments produce datasets of compliance results",
          "extensions": ["assessmentScope", "complianceScore", "findingsCount"]
        },
        "properties": [
          {"name": "assessmentId", "type": "Text", "required": true},
          {"name": "assessmentDate", "type": "DateTime", "required": true},
          {"name": "assessmentScope", "type": "Text", "cardinality": "1..*"},
          {"name": "frameworksAssessed", "type": "alz:ComplianceFramework", "cardinality": "1..*"},
          {"name": "overallScore", "type": "Number", "required": true},
          {"name": "findingsCount", "type": "Integer", "required": true}
        ]
      },
      {
        "@id": "alz:ComplianceFinding",
        "@type": "Report",
        "name": "ComplianceFinding",
        "description": "A specific non-compliance issue identified during assessment with severity and remediation guidance",
        "schemaOrgAlignment": {
          "baseType": "Report",
          "rationale": "Findings are reports of compliance status",
          "extensions": ["findingSeverity", "remediationPriority", "remediationGuidance"]
        },
        "properties": [
          {"name": "findingId", "type": "Text", "required": true},
          {"name": "findingSeverity", "type": "Text", "enum": ["critical", "high", "medium", "low", "informational"], "required": true},
          {"name": "affectedResource", "type": "alz:AzureResource", "required": true},
          {"name": "violatedControl", "type": "alz:ComplianceControl", "required": true},
          {"name": "currentState", "type": "Text", "required": true},
          {"name": "expectedState", "type": "Text", "required": true},
          {"name": "remediationGuidance", "type": "Text", "required": true},
          {"name": "remediationPriority", "type": "Integer", "required": true}
        ]
      },
      {
        "@id": "alz:InsuranceSectorRequirement",
        "@type": "GovernmentService",
        "name": "InsuranceSectorRequirement",
        "description": "A regulatory requirement specific to the insurance sector that must be addressed through cloud compliance controls",
        "schemaOrgAlignment": {
          "baseType": "GovernmentService",
          "rationale": "Regulatory requirements are government-mandated services/rules",
          "extensions": ["regulatoryBody", "enforcementDate", "penaltyForNonCompliance"]
        },
        "properties": [
          {"name": "requirementId", "type": "Text", "required": true},
          {"name": "requirementName", "type": "Text", "required": true},
          {"name": "regulatoryBody", "type": "Text", "enum": ["FCA", "PRA", "Lloyd's", "EIOPA", "ICO"], "required": true},
          {"name": "applicableRegulation", "type": "Text", "required": true},
          {"name": "enforcementDate", "type": "Date", "required": false},
          {"name": "complianceDeadline", "type": "Date", "required": false}
        ]
      }
    ],
    "relationships": [
      {
        "@id": "alz:frameworkContainsControl",
        "@type": "Property",
        "name": "frameworkContainsControl",
        "description": "A compliance framework contains multiple compliance controls as its constituent requirements",
        "domainIncludes": ["alz:ComplianceFramework"],
        "rangeIncludes": ["alz:ComplianceControl"],
        "cardinality": "1..*",
        "inverseOf": "alz:controlBelongsToFramework"
      },
      {
        "@id": "alz:controlBelongsToFramework",
        "@type": "Property",
        "name": "controlBelongsToFramework",
        "description": "A compliance control belongs to a specific compliance framework",
        "domainIncludes": ["alz:ComplianceControl"],
        "rangeIncludes": ["alz:ComplianceFramework"],
        "cardinality": "1..1",
        "inverseOf": "alz:frameworkContainsControl"
      },
      {
        "@id": "alz:controlMapsTo",
        "@type": "Property",
        "name": "controlMapsTo",
        "description": "A compliance control maps to an equivalent control in another framework via a control mapping",
        "domainIncludes": ["alz:ComplianceControl"],
        "rangeIncludes": ["alz:ControlMapping"],
        "cardinality": "0..*"
      },
      {
        "@id": "alz:mappingConnectsControls",
        "@type": "Property",
        "name": "mappingConnectsControls",
        "description": "A control mapping connects a source control to a target control across frameworks",
        "domainIncludes": ["alz:ControlMapping"],
        "rangeIncludes": ["alz:ComplianceControl"],
        "cardinality": "2..2"
      },
      {
        "@id": "alz:controlImplementedBy",
        "@type": "Property",
        "name": "controlImplementedBy",
        "description": "A compliance control is implemented by a specific Azure configuration or policy",
        "domainIncludes": ["alz:ComplianceControl"],
        "rangeIncludes": ["alz:ControlImplementation"],
        "cardinality": "1..*",
        "inverseOf": "alz:implementationSatisfiesControl"
      },
      {
        "@id": "alz:implementationSatisfiesControl",
        "@type": "Property",
        "name": "implementationSatisfiesControl",
        "description": "A control implementation satisfies the requirements of a compliance control",
        "domainIncludes": ["alz:ControlImplementation"],
        "rangeIncludes": ["alz:ComplianceControl"],
        "cardinality": "1..*",
        "inverseOf": "alz:controlImplementedBy"
      },
      {
        "@id": "alz:implementationAppliesToResource",
        "@type": "Property",
        "name": "implementationAppliesToResource",
        "description": "A control implementation applies to a specific type of Azure resource",
        "domainIncludes": ["alz:ControlImplementation"],
        "rangeIncludes": ["alz:AzureResource"],
        "cardinality": "1..*"
      },
      {
        "@id": "alz:resourceEvaluatedInAssessment",
        "@type": "Property",
        "name": "resourceEvaluatedInAssessment",
        "description": "An Azure resource is evaluated as part of a compliance assessment",
        "domainIncludes": ["alz:AzureResource"],
        "rangeIncludes": ["alz:ComplianceAssessment"],
        "cardinality": "0..*"
      },
      {
        "@id": "alz:assessmentCoversFramework",
        "@type": "Property",
        "name": "assessmentCoversFramework",
        "description": "A compliance assessment covers and evaluates against a compliance framework",
        "domainIncludes": ["alz:ComplianceAssessment"],
        "rangeIncludes": ["alz:ComplianceFramework"],
        "cardinality": "1..*"
      },
      {
        "@id": "alz:assessmentProducesFinding",
        "@type": "Property",
        "name": "assessmentProducesFinding",
        "description": "A compliance assessment produces compliance findings as results",
        "domainIncludes": ["alz:ComplianceAssessment"],
        "rangeIncludes": ["alz:ComplianceFinding"],
        "cardinality": "0..*",
        "inverseOf": "alz:findingFromAssessment"
      },
      {
        "@id": "alz:findingFromAssessment",
        "@type": "Property",
        "name": "findingFromAssessment",
        "description": "A compliance finding results from a specific assessment",
        "domainIncludes": ["alz:ComplianceFinding"],
        "rangeIncludes": ["alz:ComplianceAssessment"],
        "cardinality": "1..1",
        "inverseOf": "alz:assessmentProducesFinding"
      },
      {
        "@id": "alz:findingViolatesControl",
        "@type": "Property",
        "name": "findingViolatesControl",
        "description": "A compliance finding indicates violation of a specific compliance control",
        "domainIncludes": ["alz:ComplianceFinding"],
        "rangeIncludes": ["alz:ComplianceControl"],
        "cardinality": "1..1"
      },
      {
        "@id": "alz:findingAffectsResource",
        "@type": "Property",
        "name": "findingAffectsResource",
        "description": "A compliance finding affects a specific Azure resource",
        "domainIncludes": ["alz:ComplianceFinding"],
        "rangeIncludes": ["alz:AzureResource"],
        "cardinality": "1..1"
      },
      {
        "@id": "alz:sectorRequirementAddressedByControl",
        "@type": "Property",
        "name": "sectorRequirementAddressedByControl",
        "description": "An insurance sector requirement is addressed by compliance controls",
        "domainIncludes": ["alz:InsuranceSectorRequirement"],
        "rangeIncludes": ["alz:ComplianceControl"],
        "cardinality": "1..*"
      },
      {
        "@id": "alz:controlAddressesSectorRequirement",
        "@type": "Property",
        "name": "controlAddressesSectorRequirement",
        "description": "A compliance control addresses an insurance sector regulatory requirement",
        "domainIncludes": ["alz:ComplianceControl"],
        "rangeIncludes": ["alz:InsuranceSectorRequirement"],
        "cardinality": "0..*"
      }
    ],
    "businessRules": [
      {
        "@id": "alz:BR001",
        "name": "Mandatory MCSB Assessment",
        "description": "All ALZ implementations must be assessed against both MCSB v1 and MCSB v2",
        "ruleFormat": "IF AzureLandingZone deployed THEN ComplianceAssessment MUST cover MCSB_v1 AND MCSB_v2"
      },
      {
        "@id": "alz:BR002",
        "name": "Insurance Sector Compliance",
        "description": "Insurance advisory workloads must address FCA and PRA requirements",
        "ruleFormat": "IF InsuranceSectorRequirement.regulatoryBody IN ('FCA', 'PRA') THEN sectorRequirementAddressedByControl.count >= 1"
      },
      {
        "@id": "alz:BR003",
        "name": "Critical Finding Remediation",
        "description": "Critical severity findings must have high priority remediation",
        "ruleFormat": "IF ComplianceFinding.findingSeverity = 'critical' THEN ComplianceFinding.remediationPriority <= 1"
      },
      {
        "@id": "alz:BR004",
        "name": "Control Implementation Coverage",
        "description": "Every compliance control must have at least one implementation",
        "ruleFormat": "IF ComplianceControl exists THEN controlImplementedBy.count >= 1"
      },
      {
        "@id": "alz:BR005",
        "name": "Assessment Scope Completeness",
        "description": "Compliance assessments must cover all subscriptions in scope",
        "ruleFormat": "IF ComplianceAssessment created THEN assessmentScope MUST include all active subscriptions"
      },
      {
        "@id": "alz:BR006",
        "name": "Cross-Framework Mapping Requirement",
        "description": "Controls in primary frameworks must map to at least one other framework",
        "ruleFormat": "IF ComplianceControl.controlFamily IN ('IM', 'PA', 'NS', 'DP') THEN controlMapsTo.count >= 1"
      }
    ]
  },
