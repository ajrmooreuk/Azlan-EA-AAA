  "epics": [
    {
      "@id": "baiv:epic:sec-auth",
      "@type": "efs:Epic",
      "epicCode": "EPIC-SEC-AUTH",
      "name": "Authentication & Authorization",
      "description": "Enable secure user authentication through multiple methods with multi-tenant support",
      "businessValue": "Secure access to platform with flexible authentication options",
      "priority": "P0-Critical",
      "status": "planned",
      "pbsPackage": "PBS-1.0",
      "features": ["baiv:feat:auth-email", "baiv:feat:auth-oauth", "baiv:feat:auth-magic", "baiv:feat:auth-multitenant", "baiv:feat:auth-jwt"]
    },
    {
      "@id": "baiv:epic:sec-rls",
      "@type": "efs:Epic",
      "epicCode": "EPIC-SEC-RLS",
      "name": "Row-Level Security",
      "description": "Implement database-level tenant isolation and security controls",
      "businessValue": "Ensure tenant data cannot leak even if application bugs exist",
      "priority": "P0-Critical",
      "status": "planned",
      "pbsPackage": "PBS-2.0",
      "features": ["baiv:feat:rls-isolation", "baiv:feat:rls-context", "baiv:feat:rls-audit"]
    },
    {
      "@id": "baiv:epic:sec-collab",
      "@type": "efs:Epic",
      "epicCode": "EPIC-SEC-COLLAB",
      "name": "Collaboration & Presence",
      "description": "Real-time collaboration features including user presence, record locking, and activity tracking",
      "businessValue": "Enable team collaboration without data conflicts",
      "priority": "P1-High",
      "status": "planned",
      "pbsPackage": "PBS-3.0",
      "features": ["baiv:feat:collab-presence", "baiv:feat:collab-locking", "baiv:feat:collab-activity"]
    },
    {
      "@id": "baiv:epic:sec-cycle",
      "@type": "efs:Epic",
      "epicCode": "EPIC-SEC-CYCLE",
      "name": "Cycle State Management",
      "description": "Manage organization journey through AI Visibility stages with role-based controls",
      "businessValue": "Guide users through structured AI visibility improvement process",
      "priority": "P1-High",
      "status": "planned",
      "pbsPackage": "PBS-3.0",
      "features": ["baiv:feat:cycle-tracking", "baiv:feat:cycle-advance", "baiv:feat:cycle-health"]
    }
  ],

