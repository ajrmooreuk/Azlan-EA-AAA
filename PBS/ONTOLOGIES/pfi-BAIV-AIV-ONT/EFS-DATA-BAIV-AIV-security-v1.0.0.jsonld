  "stories": [
    {
      "@id": "baiv:story:us-a01",
      "@type": "efs:UserStory",
      "storyCode": "US-A01",
      "name": "Email/Password Signup",
      "asA": "new user",
      "iWant": "to sign up with email/password",
      "soThat": "I can access the platform",
      "feature": "baiv:feat:auth-email",
      "priority": "P0-Critical",
      "status": "planned",
      "acceptanceCriteria": [
        "Given a new user with valid email",
        "When user submits signup form",
        "Then account is created",
        "And confirmation email is sent"
      ]
    },
    {
      "@id": "baiv:story:us-a02",
      "@type": "efs:UserStory",
      "storyCode": "US-A02",
      "name": "Google OAuth Login",
      "asA": "user",
      "iWant": "to sign in with Google",
      "soThat": "I don't need another password",
      "feature": "baiv:feat:auth-oauth",
      "priority": "P1-High",
      "status": "planned",
      "acceptanceCriteria": [
        "Given Google OAuth is enabled",
        "When user clicks 'Sign in with Google'",
        "Then user is redirected to Google",
        "And upon success, JWT is issued"
      ]
    },
    {
      "@id": "baiv:story:us-a03",
      "@type": "efs:UserStory",
      "storyCode": "US-A03",
      "name": "Magic Link Sign In",
      "asA": "user",
      "iWant": "to use Magic Link",
      "soThat": "I can sign in without remembering a password",
      "feature": "baiv:feat:auth-magic",
      "priority": "P2-Medium",
      "status": "planned",
      "acceptanceCriteria": [
        "Given user enters valid email",
        "When magic link is requested",
        "Then email is sent with one-time link",
        "And clicking link authenticates user"
      ]
    },
    {
      "@id": "baiv:story:us-a04",
      "@type": "efs:UserStory",
      "storyCode": "US-A04",
      "name": "Multi-Org Switching",
      "asA": "user with multiple organizations",
      "iWant": "to switch between them easily",
      "soThat": "I can manage multiple tenants",
      "feature": "baiv:feat:auth-multitenant",
      "priority": "P1-High",
      "status": "planned",
      "acceptanceCriteria": [
        "Given user belongs to Tenant A and Tenant B",
        "When user logs in",
        "Then tenant selector is displayed",
        "And selecting Tenant A sets context to Tenant A"
      ]
    },
    {
      "@id": "baiv:story:us-a05",
      "@type": "efs:UserStory",
      "storyCode": "US-A05",
      "name": "Enterprise SSO",
      "asA": "enterprise admin",
      "iWant": "my team to use our company SSO",
      "soThat": "we maintain security standards",
      "feature": "baiv:feat:auth-multitenant",
      "priority": "P3-Future",
      "status": "future",
      "acceptanceCriteria": [
        "Given SAML SSO is configured",
        "When enterprise user attempts login",
        "Then user is redirected to company IdP",
        "And upon success, user is authenticated"
      ],
      "note": "Future capability - architecture must support without modification (FR-AUTH-08)"
    },
    {
      "@id": "baiv:story:us-s01",
      "@type": "efs:UserStory",
      "storyCode": "US-S01",
      "name": "Tenant Data Isolation",
      "asA": "Platform Owner",
      "iWant": "tenant data isolated at DB level",
      "soThat": "app bugs can't leak data",
      "feature": "baiv:feat:rls-isolation",
      "priority": "P0-Critical",
      "status": "planned",
      "acceptanceCriteria": [
        "Given User A is in Tenant A",
        "When User A queries any table with Tenant B's ID",
        "Then zero rows are returned"
      ]
    },
    {
      "@id": "baiv:story:us-s02",
      "@type": "efs:UserStory",
      "storyCode": "US-S02",
      "name": "Audit Trail",
      "asA": "Tenant Admin",
      "iWant": "audit logs",
      "soThat": "I can track who changed what",
      "feature": "baiv:feat:rls-audit",
      "priority": "P1-High",
      "status": "planned",
      "acceptanceCriteria": [
        "Given an audit_log record exists",
        "When any user attempts UPDATE or DELETE",
        "Then the operation is blocked",
        "And records are append-only"
      ]
    },
    {
      "@id": "baiv:story:us-c01",
      "@type": "efs:UserStory",
      "storyCode": "US-C01",
      "name": "View Cycle Stage",
      "asA": "Tenant Admin",
      "iWant": "to see current cycle stage",
      "soThat": "team knows what phase we're in",
      "feature": "baiv:feat:cycle-tracking",
      "priority": "P1-High",
      "status": "planned",
      "acceptanceCriteria": [
        "Given tenant has cycle state record",
        "When admin views dashboard",
        "Then current stage is displayed",
        "And stage status is visible"
      ]
    },
    {
      "@id": "baiv:story:us-c02",
      "@type": "efs:UserStory",
      "storyCode": "US-C02",
      "name": "Advance Stage",
      "asA": "Tenant Admin",
      "iWant": "to advance stages",
      "soThat": "we progress through the AI Visibility journey",
      "feature": "baiv:feat:cycle-advance",
      "priority": "P1-High",
      "status": "planned",
      "acceptanceCriteria": [
        "Given tenant is in 'audit' stage",
        "And user has 'admin' role",
        "When user calls advance_cycle_stage()",
        "Then stage becomes 'gap_analysis'",
        "And activity is logged with is_highlight=true"
      ],
      "negativeCase": {
        "scenario": "Non-admin cannot advance",
        "given": "user has 'member' role",
        "when": "user calls advance_cycle_stage()",
        "then": "operation fails with 'Permission denied'"
      }
    },
    {
      "@id": "baiv:story:us-c03",
      "@type": "efs:UserStory",
      "storyCode": "US-C03",
      "name": "Health Indicators",
      "asA": "Team Member",
      "iWant": "to see health indicators",
      "soThat": "I know if we're on track",
      "feature": "baiv:feat:cycle-health",
      "priority": "P2-Medium",
      "status": "planned",
      "acceptanceCriteria": [
        "Given cycle state has health_indicators JSONB",
        "When team member views status card",
        "Then health indicators are displayed"
      ]
    },
    {
      "@id": "baiv:story:us-l01",
      "@type": "efs:UserStory",
      "storyCode": "US-L01",
      "name": "See Online Users",
      "asA": "Analyst",
      "iWant": "to see who's online",
      "soThat": "I know who's available",
      "feature": "baiv:feat:collab-presence",
      "priority": "P1-High",
      "status": "planned",
      "acceptanceCriteria": [
        "Given multiple users are active in tenant",
        "When analyst views online badge",
        "Then list of active users is shown",
        "And users offline after 2 minutes without heartbeat"
      ]
    },
    {
      "@id": "baiv:story:us-l02",
      "@type": "efs:UserStory",
      "storyCode": "US-L02",
      "name": "Lock Gap Analysis",
      "asA": "Analyst",
      "iWant": "to lock a gap analysis while editing",
      "soThat": "others don't overwrite my work",
      "feature": "baiv:feat:collab-locking",
      "priority": "P1-High",
      "status": "planned",
      "acceptanceCriteria": [
        {
          "scenario": "Lock acquisition",
          "given": "no lock exists on gap_analysis:123",
          "when": "User A requests lock",
          "then": "lock is granted to User A"
        },
        {
          "scenario": "Lock conflict",
          "given": "User A holds lock on gap_analysis:123",
          "when": "User B requests same lock",
          "then": "User B receives 'Locked by User A'"
        }
      ]
    },
    {
      "@id": "baiv:story:us-l03",
      "@type": "efs:UserStory",
      "storyCode": "US-L03",
      "name": "Recent Activity",
      "asA": "Team Member",
      "iWant": "to see recent activity",
      "soThat": "I know what's happened",
      "feature": "baiv:feat:collab-activity",
      "priority": "P2-Medium",
      "status": "planned",
      "acceptanceCriteria": [
        "Given activities exist for tenant",
        "When team member views activity feed",
        "Then recent activities are shown paginated",
        "And highlighted events are prominent"
      ]
    }
  ],

