  "@type": "pf:Ontology",
  "@id": "gdpr:gdpr-regulatory-framework",
  "name": "GDPR Regulatory Framework Ontology",
  "description": "Ontology defining EU/UK General Data Protection Regulation concepts including articles, principles, lawful bases, data subject rights, and compliance obligations. Provides the regulatory foundation that implementation ontologies (like PII Governance) reference.",
  "version": "1.0.0",
  "oaaVersion": "6.1.0",
  "created": "2026-02-06",
  "lastModified": "2026-02-06",
