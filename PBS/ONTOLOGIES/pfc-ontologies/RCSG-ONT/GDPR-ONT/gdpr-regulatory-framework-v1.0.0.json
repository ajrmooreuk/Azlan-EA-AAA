  "metadata": {
    "domain": "Data Protection Regulation",
    "purpose": "Define canonical GDPR regulatory concepts for cross-ontology reference",
    "scope": "EU GDPR (2016/679) and UK GDPR (retained EU law)",
    "targetAudience": ["Data Protection Officers", "Compliance Officers", "Legal Teams", "Security Architects"],
    "regulatorySource": "Regulation (EU) 2016/679"
  },

  "hasDefinedTerm": [
    {
      "@type": "DefinedTerm",
      "@id": "gdpr:DataProtectionPrinciple",
      "name": "DataProtectionPrinciple",
      "description": "Core principles for processing personal data as defined in GDPR Article 5, forming the foundation of lawful data processing",
      "termCode": "DPP",
      "inDefinedTermSet": "gdpr:gdpr-regulatory-framework",
      "schemaType": "schema:DefinedTerm",
      "properties": {
        "articleReference": { "type": "string", "description": "GDPR article reference" },
        "principleCode": { "type": "string", "description": "Short code for the principle" },
        "description": { "type": "string", "description": "Full description of the principle" },
        "complianceRequirements": { "type": "array", "items": { "type": "string" }, "description": "Key requirements to demonstrate compliance" }
      },
      "instances": [
        { "@id": "gdpr:Principle-Lawfulness", "principleCode": "ART5-1-A", "articleReference": "Article 5(1)(a)", "description": "Processing must be lawful, fair and transparent", "complianceRequirements": ["Valid lawful basis", "Privacy notice", "Fair processing"] },
        { "@id": "gdpr:Principle-PurposeLimitation", "principleCode": "ART5-1-B", "articleReference": "Article 5(1)(b)", "description": "Data collected for specified, explicit and legitimate purposes only", "complianceRequirements": ["Documented purposes", "No incompatible processing"] },
        { "@id": "gdpr:Principle-DataMinimisation", "principleCode": "ART5-1-C", "articleReference": "Article 5(1)(c)", "description": "Data must be adequate, relevant and limited to what is necessary", "complianceRequirements": ["Necessity assessment", "No excessive collection"] },
        { "@id": "gdpr:Principle-Accuracy", "principleCode": "ART5-1-D", "articleReference": "Article 5(1)(d)", "description": "Personal data must be accurate and kept up to date", "complianceRequirements": ["Accuracy checks", "Correction procedures"] },
        { "@id": "gdpr:Principle-StorageLimitation", "principleCode": "ART5-1-E", "articleReference": "Article 5(1)(e)", "description": "Data kept only as long as necessary for the purposes", "complianceRequirements": ["Retention policy", "Deletion procedures"] },
        { "@id": "gdpr:Principle-IntegrityConfidentiality", "principleCode": "ART5-1-F", "articleReference": "Article 5(1)(f)", "description": "Appropriate security including protection against unauthorised processing, loss, destruction or damage", "complianceRequirements": ["Security measures", "Access controls", "Encryption"] },
        { "@id": "gdpr:Principle-Accountability", "principleCode": "ART5-2", "articleReference": "Article 5(2)", "description": "Controller must demonstrate compliance with all principles", "complianceRequirements": ["Documentation", "Policies", "Training", "Audits"] }
      ]
    },
    {
      "@type": "DefinedTerm",
      "@id": "gdpr:LawfulBasis",
      "name": "LawfulBasis",
