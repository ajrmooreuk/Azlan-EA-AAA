      "inDefinedTermSet": "gdpr:gdpr-regulatory-framework",
      "schemaType": "schema:DefinedTerm",
      "properties": {
        "articleReference": { "type": "string", "description": "GDPR article reference" },
        "rightCode": { "type": "string", "description": "Short code for the right" },
        "responseDeadline": { "type": "string", "description": "Time limit to respond to request" },
        "exemptions": { "type": "array", "items": { "type": "string" }, "description": "Situations where right may be limited" }
      },
      "instances": [
        { "@id": "gdpr:Right-Information", "rightCode": "ART13-14", "articleReference": "Articles 13-14", "description": "Right to be informed about data collection and use", "responseDeadline": "At collection", "exemptions": [] },
        { "@id": "gdpr:Right-Access", "rightCode": "ART15", "articleReference": "Article 15", "description": "Right to access personal data and supplementary information", "responseDeadline": "1 month", "exemptions": ["Manifestly unfounded", "Excessive requests"] },
        { "@id": "gdpr:Right-Rectification", "rightCode": "ART16", "articleReference": "Article 16", "description": "Right to have inaccurate data corrected", "responseDeadline": "1 month", "exemptions": [] },
        { "@id": "gdpr:Right-Erasure", "rightCode": "ART17", "articleReference": "Article 17", "description": "Right to have personal data deleted (right to be forgotten)", "responseDeadline": "1 month", "exemptions": ["Legal obligation", "Public interest", "Legal claims"] },
        { "@id": "gdpr:Right-RestrictProcessing", "rightCode": "ART18", "articleReference": "Article 18", "description": "Right to restrict processing in certain circumstances", "responseDeadline": "1 month", "exemptions": ["Storage only during restriction"] },
        { "@id": "gdpr:Right-DataPortability", "rightCode": "ART20", "articleReference": "Article 20", "description": "Right to receive data in structured, machine-readable format", "responseDeadline": "1 month", "exemptions": ["Consent or contract basis only"] },
        { "@id": "gdpr:Right-Object", "rightCode": "ART21", "articleReference": "Article 21", "description": "Right to object to processing based on legitimate interests or public task", "responseDeadline": "Immediately", "exemptions": ["Compelling legitimate grounds"] },
        { "@id": "gdpr:Right-AutomatedDecisions", "rightCode": "ART22", "articleReference": "Article 22", "description": "Right not to be subject to solely automated decision-making with legal effects", "responseDeadline": "Immediately", "exemptions": ["Contract necessity", "Explicit consent", "Legal authorisation"] }
      ]
    },
    {
