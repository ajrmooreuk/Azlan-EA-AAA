  "businessRules": [
    {
      "@id": "gdpr:rule-lawful-basis-required",
      "name": "LawfulBasisRequired",
      "description": "All personal data processing must have at least one lawful basis",
      "condition": "IF processing personal data",
      "action": "THEN at least one gdpr:LawfulBasis must apply",
      "priority": 1,
      "severity": "error",
      "enforcementLevel": "mandatory"
    },
    {
      "@id": "gdpr:rule-special-category-conditions",
      "name": "SpecialCategoryConditions",
      "description": "Processing special category data requires Article 9 condition in addition to Article 6 basis",
      "condition": "IF processing gdpr:SpecialCategoryData",
      "action": "THEN both Article 6 basis AND Article 9 condition must apply",
      "priority": 2,
      "severity": "error",
      "enforcementLevel": "mandatory"
    },
    {
      "@id": "gdpr:rule-breach-notification-72h",
      "name": "BreachNotification72Hours",
      "description": "Personal data breaches must be notified to supervisory authority within 72 hours",
      "condition": "IF personal data breach detected AND risk to rights and freedoms",
      "action": "THEN notify supervisory authority within 72 hours",
      "priority": 3,
      "severity": "error",
      "enforcementLevel": "mandatory"
    },
    {
      "@id": "gdpr:rule-dpia-high-risk",
      "name": "DPIAHighRisk",
      "description": "Data Protection Impact Assessment required for high-risk processing",
      "condition": "IF processing likely to result in high risk to rights and freedoms",
      "action": "THEN conduct DPIA before processing",
      "priority": 4,
      "severity": "error",
      "enforcementLevel": "mandatory"
    },
    {
      "@id": "gdpr:rule-respond-dsar-1-month",
      "name": "RespondDSAR1Month",
      "description": "Data subject requests must be responded to within one month",
      "condition": "IF gdpr:DataSubjectRight request received",
      "action": "THEN respond within 1 month (extendable by 2 months for complex requests)",
      "priority": 5,
      "severity": "error",
      "enforcementLevel": "mandatory"
    },
    {
      "@id": "gdpr:rule-transfer-safeguards",
