      "termCode": "LB",
      "inDefinedTermSet": "gdpr:gdpr-regulatory-framework",
      "schemaType": "schema:DefinedTerm",
      "properties": {
        "articleReference": { "type": "string", "description": "GDPR article reference" },
        "basisCode": { "type": "string", "description": "Short code for the basis" },
        "requiresConsent": { "type": "boolean", "description": "Whether this basis requires explicit consent" },
        "applicableScenarios": { "type": "array", "items": { "type": "string" }, "description": "Common scenarios where this basis applies" }
      },
      "instances": [
        { "@id": "gdpr:Basis-Consent", "basisCode": "ART6-1-A", "articleReference": "Article 6(1)(a)", "description": "Data subject has given consent to processing", "requiresConsent": true, "applicableScenarios": ["Marketing", "Optional features", "Analytics"] },
        { "@id": "gdpr:Basis-Contract", "basisCode": "ART6-1-B", "articleReference": "Article 6(1)(b)", "description": "Processing necessary for performance of a contract", "requiresConsent": false, "applicableScenarios": ["Service delivery", "Account management", "Order fulfilment"] },
        { "@id": "gdpr:Basis-LegalObligation", "basisCode": "ART6-1-C", "articleReference": "Article 6(1)(c)", "description": "Processing necessary to comply with a legal obligation", "requiresConsent": false, "applicableScenarios": ["Tax records", "Employment law", "Regulatory reporting"] },
        { "@id": "gdpr:Basis-VitalInterests", "basisCode": "ART6-1-D", "articleReference": "Article 6(1)(d)", "description": "Processing necessary to protect vital interests", "requiresConsent": false, "applicableScenarios": ["Medical emergencies", "Life-threatening situations"] },
        { "@id": "gdpr:Basis-PublicTask", "basisCode": "ART6-1-E", "articleReference": "Article 6(1)(e)", "description": "Processing necessary for public interest or official authority", "requiresConsent": false, "applicableScenarios": ["Government functions", "Public services"] },
        { "@id": "gdpr:Basis-LegitimateInterests", "basisCode": "ART6-1-F", "articleReference": "Article 6(1)(f)", "description": "Processing necessary for legitimate interests (requires balancing test)", "requiresConsent": false, "applicableScenarios": ["Fraud prevention", "Network security", "Business operations"] }
      ]
    },
    {
      "@type": "DefinedTerm",
      "@id": "gdpr:DataSubjectRight",
