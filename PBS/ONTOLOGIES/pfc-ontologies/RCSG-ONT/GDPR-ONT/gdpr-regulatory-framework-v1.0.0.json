        { "@id": "gdpr:Role-Controller", "roleCode": "CTRL", "articleReference": "Article 4(7)", "description": "Determines purposes and means of processing personal data", "keyObligations": ["Compliance with principles", "Implement appropriate measures", "Demonstrate accountability", "Notify breaches"] },
        { "@id": "gdpr:Role-Processor", "roleCode": "PROC", "articleReference": "Article 4(8)", "description": "Processes personal data on behalf of the controller", "keyObligations": ["Process only on documented instructions", "Ensure confidentiality", "Assist controller", "Delete/return data"] },
        { "@id": "gdpr:Role-JointController", "roleCode": "JCTRL", "articleReference": "Article 26", "description": "Two or more controllers jointly determine purposes and means", "keyObligations": ["Transparent arrangement", "Define responsibilities", "Provide contact point"] },
