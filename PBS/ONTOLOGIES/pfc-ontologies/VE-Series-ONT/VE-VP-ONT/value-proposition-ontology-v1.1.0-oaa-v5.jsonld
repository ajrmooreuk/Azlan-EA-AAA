  "businessRules": [
    {
      "ruleId": "BR-VP-001",
      "name": "VP Requires Primary Statement",
      "condition": "IF a ValueProposition is created",
      "constraint": "THEN primaryStatement MUST be defined with length >= 50 characters",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-002",
      "name": "VP Must Target ICP",
      "condition": "IF a ValueProposition is created",
      "constraint": "THEN it MUST have at least one targetsICP relationship to IdealCustomerProfile",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-003",
      "name": "VP Must Address Problem",
      "condition": "IF a ValueProposition is created",
      "constraint": "THEN it MUST have at least one addressesProblem relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-004",
      "name": "VP Must Have Evidence",
      "condition": "IF a ValueProposition has status = 'Validated' OR status = 'Active'",
      "constraint": "THEN it MUST have at least one supportedByEvidence relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-005",
      "name": "VP Must Align to VSOM Objective",
      "condition": "IF a ValueProposition is created",
      "constraint": "THEN it MUST have at least one alignsToObjective relationship to vsom:ObjectivesComponent",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-006",
      "name": "VP Must Be Org Context Scoped",
      "condition": "IF a ValueProposition is created",
      "constraint": "THEN organizationContextRef MUST reference a valid org:OrganizationContext",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-007",
      "name": "Problem Must Have Pain Points",
      "condition": "IF a Problem is created",
      "constraint": "THEN it MUST have at least one problemHasPainPoint relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-008",
      "name": "Problem Must Be Validated",
      "condition": "IF a Problem is used in active ValueProposition",
      "constraint": "THEN it MUST have at least one problemValidatedBy relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-009",
      "name": "Critical Problem Maps to Epic",
      "condition": "IF Problem.severity = 'Critical' OR Problem.severity = 'Major'",
      "constraint": "THEN Problem SHOULD have mapsToEpic relationship to efs:Epic",
      "severity": "warning"
    },
    {
      "ruleId": "BR-VP-010",
      "name": "Solution Must Address Problem",
      "condition": "IF a Solution is created",
      "constraint": "THEN it MUST have at least one solutionAddressesProblem relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-011",
      "name": "Solution Must Deliver Benefit",
      "condition": "IF a Solution is created",
      "constraint": "THEN it MUST have at least one solutionDeliversBenefit relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-012",
      "name": "Solution Must Have Differentiator",
      "condition": "IF a Solution is created",
      "constraint": "THEN it MUST have at least one solutionDifferentiatedBy relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-013",
      "name": "Benefit Must Mitigate Pain",
      "condition": "IF a Benefit is created",
      "constraint": "THEN it MUST have at least one benefitMitigatesPain relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-014",
      "name": "ICP Must Have Stakeholder",
      "condition": "IF an IdealCustomerProfile is created",
      "constraint": "THEN it MUST have at least one icpHasStakeholder relationship",
      "severity": "error"
    },
    {
      "ruleId": "BR-VP-015",
      "name": "ICP Should Map to Persona",
      "condition": "IF an IdealCustomerProfile is used in active ValueProposition",
      "constraint": "THEN it SHOULD have at least one manifestsAsPersona relationship to efs:Persona",
      "severity": "warning"
    },
    {
      "ruleId": "BR-VP-016",
      "name": "Benefit Should Realize in Story",
      "condition": "IF a Benefit is in active ValueProposition",
      "constraint": "THEN it SHOULD have at least one realizesInStory relationship to efs:UserStory",
      "severity": "warning"
    }
  ],

