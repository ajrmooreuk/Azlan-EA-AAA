{
  "@context": {
    "@vocab": "https://schema.org/",
    "oaa": "https://oaa-ontology.org/v5/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "vp": "https://oaa-ontology.org/v5/value-proposition/",
    "vsom": "https://oaa-ontology.org/v5/vsom/",
    "org": "https://oaa-ontology.org/v5/organization/",
    "efs": "https://oaa-ontology.org/v5/efs/",
    "pmf": "https://oaa-ontology.org/v5/pmf/"
  },
  "@id": "https://oaa-ontology.org/v5/vp/schema",
  "@type": "owl:Ontology",
  "name": "Value Proposition Ontology",
  "alternateName": "VP-ONT",
  "description": "Comprehensive ontology for developing, validating, and articulating value propositions. Links VSOM strategic layer to EFS execution layer through ICP-driven problem-solution mapping. Scoped by OrganizationContext for PFI-instance deployment.",
  "oaa:schemaVersion": "5.0.0",
  "oaa:moduleVersion": "1.1.0",
  "oaa:previousVersion": "1.0.0",
  "oaa:domain": "Value Engineering, Product Strategy, Customer Value",
  "oaa:status": "Production",
  "dateCreated": "2025-10-08",
  "dateModified": "2026-02-02",
  "creator": {
    "@type": "Organization",
    "name": "PFC Core Capability Framework"
  },

  "oaa:imports": [
    {
      "@id": "vsom:schema",
      "name": "VSOM-ONT",
      "version": "2.1.0",
      "importedEntities": ["vsom:VSOMFramework", "vsom:StrategyComponent", "vsom:ObjectivesComponent", "vsom:Outcome", "vsom:BusinessImpact"],
      "importedRelationships": ["vsom:hasStrategicContext", "vsom:drivesOutcome"]
    },
    {
      "@id": "org:schema",
      "name": "ORG-ONT",
      "version": "2.1.0",
      "importedEntities": ["org:Organization", "org:OrganizationContext"],
      "importedRelationships": ["org:hasContext"]
    },
    {
      "@id": "efs:schema",
      "name": "EFS-ONT",
      "version": "3.0.0",
      "importedEntities": ["efs:Epic", "efs:Feature", "efs:UserStory", "efs:Persona", "efs:AcceptanceCriterion"],
      "importedRelationships": ["efs:hasFeature", "efs:hasStory"]
    }
  ],

  "oaa:joinPatterns": [
    {
      "patternId": "JP-VP-001",
      "name": "VSOM-to-VP Strategic Alignment",
      "description": "Value Proposition aligns to VSOM strategic objectives",
      "joinPath": "vsom:ObjectivesComponent → vp:alignsToObjective ← vp:ValueProposition",
      "useCase": "Strategic objective drives value proposition definition"
    },
    {
      "patternId": "JP-VP-002",
      "name": "VP-to-EFS Execution Mapping",
      "description": "Value Proposition problems map to EFS execution items",
      "joinPath": "vp:Problem → vp:mapsToEpic → efs:Epic → efs:hasFeature → efs:Feature",
      "useCase": "Problem severity determines Epic/Feature decomposition"
    },
    {
      "patternId": "JP-VP-003",
      "name": "ICP-to-Persona Bridge",
      "description": "Ideal Customer Profile maps to EFS Personas for story writing",
      "joinPath": "vp:IdealCustomerProfile → vp:manifestsAsPersona → efs:Persona",
      "useCase": "ICP defines persona characteristics for user stories"
    },
    {
      "patternId": "JP-VP-004",
      "name": "Org-Context Scoping",
      "description": "Value Proposition scoped by OrganizationContext for PFI-instance",
      "joinPath": "org:OrganizationContext → vp:hasValueProposition → vp:ValueProposition",
      "useCase": "Each PFI-instance has org-specific value propositions"
    },
    {
      "patternId": "JP-VP-005",
      "name": "Benefit-to-Story Mapping",
      "description": "VP Benefits map to UserStory 'So That' outcomes",
      "joinPath": "vp:Benefit → vp:realizesInStory → efs:UserStory.soThat",
      "useCase": "Benefits define story acceptance outcomes"
    }
  ],

  "entities": [
    {
      "@id": "vp:ValueProposition",
      "@type": "rdfs:Class",
      "name": "ValueProposition",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Complete articulation of how solution creates value for target customers by addressing their problems better than alternatives. Primary statement format: For [ICP] who [NEED], our [SOLUTION] provides [BENEFITS] unlike [ALTERNATIVES] because [DIFFERENTIATORS]. Scoped by OrganizationContext.",
      "oaa:properties": [
        {"name": "vpId", "type": "xsd:string", "required": true, "pattern": "vp:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "name", "type": "xsd:string", "required": true, "schemaOrgMapping": "schema:name"},
        {"name": "primaryStatement", "type": "xsd:string", "required": true, "minLength": 50, "description": "Core value proposition statement"},
        {"name": "alternativeStatement", "type": "xsd:string", "required": false, "description": "Alternative articulation format"},
        {"name": "keyBenefits", "type": "xsd:string", "required": true, "description": "Summary of key benefits delivered"},
        {"name": "uniqueDifferentiators", "type": "xsd:string", "required": true, "description": "What makes this solution unique"},
        {"name": "organizationContextRef", "type": "xsd:string", "required": true, "description": "Reference to org:OrganizationContext for PFI-instance scoping"},
        {"name": "status", "type": "xsd:string", "required": true, "enum": ["Draft", "Validated", "Active", "Deprecated"]},
        {"name": "validationDate", "type": "xsd:date", "required": false, "description": "Date VP was validated with customers"}
      ]
    },
    {
      "@id": "vp:IdealCustomerProfile",
      "@type": "rdfs:Class",
      "name": "IdealCustomerProfile",
      "rdfs:subClassOf": "schema:Audience",
      "description": "Detailed specification of ideal target customer including demographics, psychographics, firmographics, and willingness to pay. Maps to EFS Personas for execution. ICP at org level, Persona at product/feature level.",
      "oaa:properties": [
        {"name": "icpId", "type": "xsd:string", "required": true, "pattern": "icp:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "name", "type": "xsd:string", "required": true, "schemaOrgMapping": "schema:name"},
        {"name": "customerType", "type": "xsd:string", "required": true, "enum": ["B2B", "B2C", "B2B2C"]},
        {"name": "segmentDescription", "type": "xsd:string", "required": true, "minLength": 20, "description": "Detailed segment characteristics"},
        {"name": "demographics", "type": "xsd:string", "required": false, "description": "Industry, size, location, revenue range"},
        {"name": "firmographics", "type": "xsd:string", "required": false, "description": "Company structure, tech stack, growth stage"},
        {"name": "psychographics", "type": "xsd:string", "required": false, "description": "Values, goals, behaviors, decision patterns"},
        {"name": "willingnessToPayIndicator", "type": "xsd:string", "required": true, "enum": ["High", "Medium", "Low"]},
        {"name": "marketSize", "type": "xsd:decimal", "required": false, "description": "Addressable market size for this ICP"},
        {"name": "acquisitionCost", "type": "xsd:decimal", "required": false, "description": "Estimated customer acquisition cost"},
        {"name": "lifetimeValue", "type": "xsd:decimal", "required": false, "description": "Estimated customer lifetime value"}
      ]
    },
    {
      "@id": "vp:Problem",
      "@type": "rdfs:Class",
      "name": "Problem",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Challenge or unmet need experienced by ICP creating negative consequences. Problems with high severity map to EFS Epics; medium/low severity map to Features. Analyzed across functional, economic, emotional, social dimensions.",
      "oaa:properties": [
        {"name": "problemId", "type": "xsd:string", "required": true, "pattern": "prob:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "problemStatement", "type": "xsd:string", "required": true, "minLength": 30, "description": "[ICP] struggles with [CHALLENGE] when [CONTEXT], resulting in [CONSEQUENCES]"},
        {"name": "problemType", "type": "xsd:string", "required": true, "enum": ["Functional", "Economic", "Emotional", "Social"]},
        {"name": "severity", "type": "xsd:string", "required": true, "enum": ["Critical", "Major", "Moderate", "Minor"], "description": "Critical/Major → Epic; Moderate/Minor → Feature"},
        {"name": "frequency", "type": "xsd:string", "required": true, "enum": ["Constant", "Frequent", "Occasional", "Rare"]},
        {"name": "isGrowing", "type": "xsd:boolean", "required": false, "description": "Whether problem is growing over time"},
        {"name": "marketSizeImpact", "type": "xsd:decimal", "required": false, "description": "TAM/SAM/SOM affected by this problem"}
      ]
    },
    {
      "@id": "vp:PainPoint",
      "@type": "rdfs:Class",
      "name": "PainPoint",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Specific source of difficulty within a Problem. Pain points drive feature prioritization. Dimensions: Functional (task blockers), Economic (cost impacts), Emotional (frustrations), Social (status/perception).",
      "oaa:properties": [
        {"name": "painId", "type": "xsd:string", "required": true, "pattern": "pain:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "painDescription", "type": "xsd:string", "required": true, "minLength": 20, "description": "Detailed pain point description"},
        {"name": "painDimension", "type": "xsd:string", "required": true, "enum": ["Functional", "Economic", "Emotional", "Social"]},
        {"name": "customerImpact", "type": "xsd:string", "required": true, "description": "How this pain affects the customer"},
        {"name": "quantifiedImpact", "type": "xsd:decimal", "required": false, "description": "Quantified cost, time, or revenue impact"},
        {"name": "impactUnit", "type": "xsd:string", "required": false, "enum": ["Currency", "Hours", "Percentage", "Count"]}
      ]
    },
    {
      "@id": "vp:Gain",
      "@type": "rdfs:Class",
      "name": "Gain",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Desired outcome or positive result ICP wants to achieve. Gains map to EFS Features. Represents the 'jobs to be done' perspective - what customer wants to accomplish.",
      "oaa:properties": [
        {"name": "gainId", "type": "xsd:string", "required": true, "pattern": "gain:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "gainDescription", "type": "xsd:string", "required": true, "minLength": 20, "description": "What the customer wants to achieve"},
        {"name": "gainType", "type": "xsd:string", "required": true, "enum": ["Required", "Expected", "Desired", "Unexpected"]},
        {"name": "gainCategory", "type": "xsd:string", "required": true, "enum": ["Functional", "Social", "Emotional", "Economic"]},
        {"name": "priority", "type": "xsd:string", "required": true, "enum": ["Critical", "High", "Medium", "Low"]},
        {"name": "quantifiedValue", "type": "xsd:decimal", "required": false, "description": "Quantified value of achieving this gain"}
      ]
    },
    {
      "@id": "vp:Solution",
      "@type": "rdfs:Class",
      "name": "Solution",
      "rdfs:subClassOf": "schema:Product",
      "description": "Product, service, or approach addressing identified problems and delivering gains. Must address at least one Problem and deliver at least one Benefit with clear differentiators.",
      "oaa:properties": [
        {"name": "solutionId", "type": "xsd:string", "required": true, "pattern": "sol:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "name", "type": "xsd:string", "required": true, "schemaOrgMapping": "schema:name"},
        {"name": "description", "type": "xsd:string", "required": true, "minLength": 30, "schemaOrgMapping": "schema:description"},
        {"name": "coreFunctionality", "type": "xsd:string", "required": true, "description": "Primary capabilities of solution"},
        {"name": "deliveryMethod", "type": "xsd:string", "required": true, "enum": ["SaaS", "OnPremise", "Hybrid", "Service", "Platform", "Marketplace"]},
        {"name": "pricingModel", "type": "xsd:string", "required": false, "enum": ["Subscription", "Usage", "Freemium", "License", "Custom"]},
        {"name": "timeToValue", "type": "xsd:duration", "required": false, "description": "Time for customer to realize value"}
      ]
    },
    {
      "@id": "vp:Benefit",
      "@type": "rdfs:Class",
      "name": "Benefit",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Positive outcome delivered to customers by solution. Benefits map to EFS UserStory 'So That' clause. Must mitigate at least one PainPoint. Quantifiable benefits preferred.",
      "oaa:properties": [
        {"name": "benefitId", "type": "xsd:string", "required": true, "pattern": "ben:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "benefitDescription", "type": "xsd:string", "required": true, "minLength": 20, "description": "What value is delivered"},
        {"name": "benefitType", "type": "xsd:string", "required": true, "enum": ["Quantifiable", "Qualitative"]},
        {"name": "benefitCategory", "type": "xsd:string", "required": true, "enum": ["CostReduction", "RevenueGrowth", "TimeSavings", "RiskReduction", "ExperienceImprovement", "StrategicAdvantage", "ComplianceAssurance"]},
        {"name": "quantifiedValue", "type": "xsd:decimal", "required": false, "description": "Quantified benefit value"},
        {"name": "measurementMethod", "type": "xsd:string", "required": false, "description": "How benefit is measured"},
        {"name": "realizationTimeframe", "type": "xsd:duration", "required": false, "description": "Time to realize this benefit"}
      ]
    },
    {
      "@id": "vp:Differentiator",
      "@type": "rdfs:Class",
      "name": "Differentiator",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Unique aspect distinguishing solution from alternatives. Maps to EFS Capability. Assessed for defensibility and customer importance.",
      "oaa:properties": [
        {"name": "differentiatorId", "type": "xsd:string", "required": true, "pattern": "diff:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "differentiatorDescription", "type": "xsd:string", "required": true, "minLength": 20, "description": "What makes this unique"},
        {"name": "differentiatorType", "type": "xsd:string", "required": true, "enum": ["Technology", "Approach", "Capability", "BusinessModel", "Experience", "Integration", "Data", "Network"]},
        {"name": "competitiveAdvantage", "type": "xsd:string", "required": true, "description": "Specific advantage over alternatives"},
        {"name": "defensibility", "type": "xsd:string", "required": true, "enum": ["High", "Medium", "Low"], "description": "How difficult to copy"},
        {"name": "customerImportance", "type": "xsd:string", "required": true, "enum": ["High", "Medium", "Low"], "description": "How important to target customers"}
      ]
    },
    {
      "@id": "vp:CompetitiveAlternative",
      "@type": "rdfs:Class",
      "name": "CompetitiveAlternative",
      "rdfs:subClassOf": "schema:Product",
      "description": "Existing product, service, or approach competing with proposed solution. Includes direct competitors, substitutes, and 'do nothing' option.",
      "oaa:properties": [
        {"name": "alternativeId", "type": "xsd:string", "required": true, "pattern": "alt:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "name", "type": "xsd:string", "required": true, "schemaOrgMapping": "schema:name"},
        {"name": "alternativeType", "type": "xsd:string", "required": true, "enum": ["DirectCompetitor", "Substitute", "Workaround", "DoNothing"]},
        {"name": "marketShare", "type": "xsd:decimal", "required": false, "description": "Estimated market share %"},
        {"name": "strengths", "type": "xsd:string", "required": true, "description": "Key strengths of this alternative"},
        {"name": "weaknesses", "type": "xsd:string", "required": true, "description": "Key weaknesses of this alternative"},
        {"name": "pricingComparison", "type": "xsd:string", "required": false, "enum": ["Higher", "Similar", "Lower"]}
      ]
    },
    {
      "@id": "vp:ValidationEvidence",
      "@type": "rdfs:Class",
      "name": "ValidationEvidence",
      "rdfs:subClassOf": "schema:CreativeWork",
      "description": "Data, research, or information validating problem existence and solution fit. Every Problem must have at least one ValidationEvidence.",
      "oaa:properties": [
        {"name": "evidenceId", "type": "xsd:string", "required": true, "pattern": "evid:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "evidenceType", "type": "xsd:string", "required": true, "enum": ["CustomerInterview", "Survey", "MarketResearch", "CompetitiveAnalysis", "IndustryReport", "InternalData", "UsageAnalytics", "ABTest"]},
        {"name": "evidenceSource", "type": "xsd:string", "required": true, "description": "Source of evidence"},
        {"name": "evidenceDate", "type": "xsd:date", "required": true, "description": "When collected"},
        {"name": "evidenceDescription", "type": "xsd:string", "required": true, "minLength": 20, "description": "What evidence shows"},
        {"name": "evidenceStrength", "type": "xsd:string", "required": true, "enum": ["Strong", "Moderate", "Weak"]},
        {"name": "sampleSize", "type": "xsd:integer", "required": false, "description": "Sample size if applicable"}
      ]
    },
    {
      "@id": "vp:SuccessMetric",
      "@type": "rdfs:Class",
      "name": "SuccessMetric",
      "rdfs:subClassOf": "schema:PropertyValue",
      "description": "Measurable indicator tracking value realization. Maps to EFS AcceptanceCriterion for testable validation.",
      "oaa:properties": [
        {"name": "metricId", "type": "xsd:string", "required": true, "pattern": "sm:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "name", "type": "xsd:string", "required": true, "schemaOrgMapping": "schema:name"},
        {"name": "description", "type": "xsd:string", "required": true, "minLength": 20, "description": "What metric measures"},
        {"name": "baseline", "type": "xsd:decimal", "required": true, "description": "Current/baseline value"},
        {"name": "target", "type": "xsd:decimal", "required": true, "description": "Target value"},
        {"name": "measurementMethod", "type": "xsd:string", "required": true, "description": "How measured"},
        {"name": "measurementFrequency", "type": "xsd:duration", "required": false, "description": "How often measured"}
      ]
    },
    {
      "@id": "vp:Stakeholder",
      "@type": "rdfs:Class",
      "name": "Stakeholder",
      "rdfs:subClassOf": "schema:Role",
      "description": "Person or group with specific role in purchase/adoption decision. Stakeholder roles inform EFS Persona definitions.",
      "oaa:properties": [
        {"name": "stakeholderId", "type": "xsd:string", "required": true, "pattern": "sh:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "name", "type": "xsd:string", "required": true, "schemaOrgMapping": "schema:name"},
        {"name": "stakeholderRole", "type": "xsd:string", "required": true, "enum": ["PrimaryUser", "DecisionMaker", "Influencer", "Beneficiary", "Champion", "Blocker", "EconomicBuyer", "TechnicalEvaluator"]},
        {"name": "influenceLevel", "type": "xsd:string", "required": true, "enum": ["High", "Medium", "Low"]},
        {"name": "interests", "type": "xsd:string", "required": true, "description": "Key interests and priorities"},
        {"name": "objections", "type": "xsd:string", "required": false, "description": "Likely objections or concerns"}
      ]
    },
    {
      "@id": "vp:MessagingFramework",
      "@type": "rdfs:Class",
      "name": "MessagingFramework",
      "rdfs:subClassOf": "schema:CreativeWork",
      "description": "Structured approach for communicating value proposition to different stakeholders across channels.",
      "oaa:properties": [
        {"name": "frameworkId", "type": "xsd:string", "required": true, "pattern": "msg:[a-z0-9-]+", "schemaOrgMapping": "schema:identifier"},
        {"name": "primaryMessage", "type": "xsd:string", "required": true, "minLength": 20, "description": "Single most important value point"},
        {"name": "supportingMessages", "type": "xsd:string", "required": true, "description": "Additional reinforcing messages"},
        {"name": "emotionalHooks", "type": "xsd:string", "required": false, "description": "Emotional connection elements"},
        {"name": "useCustomerLanguage", "type": "xsd:boolean", "required": true, "default": true, "description": "Uses customer language vs internal terminology"}
      ]
    }
  ],

  "relationships": [
    {
      "@id": "vp:belongsToOrgContext",
      "@type": "owl:ObjectProperty",
      "name": "Belongs To Org Context",
      "description": "Value Proposition scoped by OrganizationContext for PFI-instance deployment",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["org:OrganizationContext"],
      "oaa:cardinality": {"min": 1, "max": 1},
      "oaa:crossOntologyRef": "ORG-ONT"
    },
    {
      "@id": "vp:alignsToObjective",
      "@type": "owl:ObjectProperty",
      "name": "Aligns To Objective",
      "description": "VP aligns to VSOM strategic objective - lineage from strategy to value",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vsom:ObjectivesComponent"],
      "oaa:cardinality": {"min": 1, "max": "*"},
      "oaa:crossOntologyRef": "VSOM-ONT"
    },
    {
      "@id": "vp:drivesOutcome",
      "@type": "owl:ObjectProperty",
      "name": "Drives Outcome",
      "description": "VP drives VSOM business outcomes",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vsom:Outcome"],
      "oaa:cardinality": {"min": 0, "max": "*"},
      "oaa:crossOntologyRef": "VSOM-ONT"
    },
    {
      "@id": "vp:targetsICP",
      "@type": "owl:ObjectProperty",
      "name": "Targets ICP",
      "description": "VP targets one or more Ideal Customer Profiles",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vp:IdealCustomerProfile"],
      "oaa:cardinality": {"min": 1, "max": "*"}
    },
    {
      "@id": "vp:addressesProblem",
      "@type": "owl:ObjectProperty",
      "name": "Addresses Problem",
      "description": "VP addresses customer problems",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vp:Problem"],
      "oaa:cardinality": {"min": 1, "max": "*"}
    },
    {
      "@id": "vp:offeredThrough",
      "@type": "owl:ObjectProperty",
      "name": "Offered Through",
      "description": "VP delivered through solution",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vp:Solution"],
      "oaa:cardinality": {"min": 1, "max": 1}
    },
    {
      "@id": "vp:supportedByEvidence",
      "@type": "owl:ObjectProperty",
      "name": "Supported By Evidence",
      "description": "VP supported by validation evidence",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vp:ValidationEvidence"],
      "oaa:cardinality": {"min": 1, "max": "*"}
    },
    {
      "@id": "vp:communicatedVia",
      "@type": "owl:ObjectProperty",
      "name": "Communicated Via",
      "description": "VP communicated through messaging framework",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vp:MessagingFramework"],
      "oaa:cardinality": {"min": 0, "max": 1}
    },
    {
      "@id": "vp:differentiatesFrom",
      "@type": "owl:ObjectProperty",
      "name": "Differentiates From",
      "description": "VP differentiates from competitive alternatives",
      "domainIncludes": ["vp:ValueProposition"],
      "rangeIncludes": ["vp:CompetitiveAlternative"],
      "oaa:cardinality": {"min": 1, "max": "*"}
    },
    {
      "@id": "vp:icpHasStakeholder",
      "@type": "owl:ObjectProperty",
      "name": "ICP Has Stakeholder",
      "description": "ICP includes stakeholder roles",
      "domainIncludes": ["vp:IdealCustomerProfile"],
      "rangeIncludes": ["vp:Stakeholder"],
      "oaa:cardinality": {"min": 1, "max": "*"}
    },
    {
      "@id": "vp:manifestsAsPersona",
      "@type": "owl:ObjectProperty",
      "name": "Manifests As Persona",
      "description": "ICP manifests as EFS Persona for user story writing - CRITICAL BRIDGE",
      "domainIncludes": ["vp:IdealCustomerProfile"],
      "rangeIncludes": ["efs:Persona"],
      "oaa:cardinality": {"min": 1, "max": "*"},
      "oaa:crossOntologyRef": "EFS-ONT"
    },
    {
      "@id": "vp:problemHasPainPoint",
      "@type": "owl:ObjectProperty",
      "name": "Problem Has Pain Point",
      "description": "Problem composed of pain points",
      "domainIncludes": ["vp:Problem"],
      "rangeIncludes": ["vp:PainPoint"],
      "oaa:cardinality": {"min": 1, "max": "*"}
    },
    {
      "@id": "vp:problemValidatedBy",
      "@type": "owl:ObjectProperty",
      "name": "Problem Validated By",
      "description": "Problem validated by evidence",
      "domainIncludes": ["vp:Problem"],
      "rangeIncludes": ["vp:ValidationEvidence"],
      "oaa:cardinality": {"min": 1, "max": "*"}
    },
    {
      "@id": "vp:mapsToEpic",
      "@type": "owl:ObjectProperty",
      "name": "Maps To Epic",
