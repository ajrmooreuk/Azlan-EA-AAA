      "efs:properties": [
        {
          "@id": "efs:taskType",
          "@type": "PropertyValue",
          "name": "Task Type",
          "description": "Classification of task",
          "valueReference": "efs:TaskType"
        },
        {
          "@id": "efs:estimatedHours",
          "@type": "QuantitativeValue",
          "name": "Estimated Hours",
          "unitText": "hours"
        },
        {
          "@id": "efs:actualHours",
          "@type": "QuantitativeValue",
          "name": "Actual Hours",
          "unitText": "hours"
        },
        {
          "@id": "efs:assignee",
          "@type": "PropertyValue",
          "name": "Assignee",
          "valueReference": "schema:Person"
        }
      ]
    },

    {
      "@id": "efs:AcceptanceCriterion",
