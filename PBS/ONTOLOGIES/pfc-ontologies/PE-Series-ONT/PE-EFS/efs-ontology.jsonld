          "valueReference": "xsd:string"
        },
        {
          "@id": "efs:validationMethod",
          "@type": "PropertyValue",
          "name": "Validation Method",
          "description": "How hypothesis will be tested",
          "valueReference": "efs:ValidationMethod"
        },
        {
          "@id": "efs:successCriteria",
          "@type": "PropertyValue",
          "name": "Success Criteria",
