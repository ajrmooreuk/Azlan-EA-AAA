{
  "@context": {
    "@vocab": "https://schema.org/",
    "efs": "https://platformcore.io/ontology/efs/",
    "vsom": "https://platformcore.io/ontology/vsom/",
    "pmf": "https://platformcore.io/ontology/pmf/",
    "gtm": "https://platformcore.io/ontology/gtm/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "skos": "http://www.w3.org/2004/02/skos/core#"
  },
  "@id": "efs:EpicFeaturesStoriesOntology",
  "@type": "DefinedTermSet",
  "name": "Epic-Features-Stories Ontology",
  "description": "Modular ontology for idea-to-execution specification management, connecting Value Proposition to Product Delivery",
  "version": "1.0.0",
  "dateCreated": "2026-01-28",
  "creator": {
    "@type": "Organization",
    "@id": "efs:PlatformCore"
  },
  
  "hasDefinedTerm": [
    {
      "@id": "efs:BacklogItem",
      "@type": "Class",
      "name": "Backlog Item",
      "description": "Abstract base class for all specification items in the product backlog hierarchy",
      "rdfs:subClassOf": "schema:CreativeWork",
      "skos:note": "Foundation class - Epic, Feature, Story inherit from this",
      "efs:moduleBinding": ["efs:BacklogManagement"],
      "efs:properties": [
        {
          "@id": "efs:backlogItemId",
          "@type": "PropertyValue",
          "name": "Backlog Item ID",
          "description": "Unique identifier within the backlog system",
          "valueReference": "xsd:string"
        },
        {
          "@id": "efs:priority",
          "@type": "PropertyValue", 
          "name": "Priority",
          "description": "Relative importance for scheduling",
          "valueReference": "efs:PriorityLevel"
        },
        {
          "@id": "efs:status",
          "@type": "PropertyValue",
          "name": "Status",
          "description": "Current state in the workflow",
          "valueReference": "efs:BacklogItemStatus"
        },
        {
          "@id": "efs:estimate",
          "@type": "QuantitativeValue",
          "name": "Estimate",
          "description": "Effort estimation in chosen unit"
        },
        {
          "@id": "efs:valueScore",
          "@type": "QuantitativeValue",
          "name": "Value Score",
          "description": "Quantified business value contribution"
        },
        {
          "@id": "efs:riskScore",
          "@type": "QuantitativeValue",
          "name": "Risk Score",
          "description": "Implementation risk assessment"
        }
      ]
    },

    {
      "@id": "efs:Epic",
      "@type": "Class",
      "name": "Epic",
      "description": "Large body of work decomposable into features; represents strategic initiative at product level",
      "rdfs:subClassOf": "efs:BacklogItem",
      "skos:broader": "schema:Project",
      "efs:moduleBinding": ["efs:BacklogManagement", "efs:StrategicAlignment"],
      "efs:integrationPoint": {
        "vsom:StrategicObjective": "efs:alignsToObjective",
        "vsom:StrategicInitiative": "efs:implementsInitiative"
      },
      "efs:properties": [
        {
          "@id": "efs:epicTheme",
          "@type": "PropertyValue",
          "name": "Epic Theme",
          "description": "Strategic theme or capability area",
          "valueReference": "efs:Theme"
        },
        {
          "@id": "efs:businessOutcome",
          "@type": "PropertyValue",
          "name": "Business Outcome",
          "description": "Expected business result when epic is delivered",
          "valueReference": "efs:Outcome"
        },
        {
          "@id": "efs:targetRelease",
          "@type": "PropertyValue",
          "name": "Target Release",
          "description": "Planned release for epic completion",
          "valueReference": "efs:Release"
        },
        {
          "@id": "efs:hasFeature",
          "@type": "PropertyValue",
          "name": "Has Features",
          "description": "Features comprising this epic",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:Feature"
          }
        },
        {
          "@id": "efs:hypotheses",
          "@type": "PropertyValue",
          "name": "Hypotheses",
          "description": "Testable assumptions underlying the epic value",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:Hypothesis"
          }
        },
        {
          "@id": "efs:mvpScope",
          "@type": "PropertyValue",
          "name": "MVP Scope",
          "description": "Minimum features required for initial value delivery",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:Feature"
          }
        }
      ]
    },

    {
      "@id": "efs:Feature",
      "@type": "Class",
      "name": "Feature",
      "description": "Distinct functionality delivering user or business value; decomposable into stories",
      "rdfs:subClassOf": "efs:BacklogItem",
      "skos:broader": "schema:SoftwareApplication",
      "efs:moduleBinding": ["efs:BacklogManagement", "efs:CapabilityDelivery"],
      "efs:integrationPoint": {
        "efs:Epic": "efs:belongsToEpic",
        "pmf:ValueProposition": "efs:deliversValue",
        "efs:Capability": "efs:enablesCapability"
      },
      "efs:properties": [
        {
          "@id": "efs:featureType",
          "@type": "PropertyValue",
          "name": "Feature Type",
          "description": "Classification of feature purpose",
          "valueReference": "efs:FeatureType"
        },
        {
          "@id": "efs:benefitHypothesis",
          "@type": "PropertyValue",
          "name": "Benefit Hypothesis",
          "description": "Statement of expected benefit to validate",
          "valueReference": "xsd:string"
        },
        {
          "@id": "efs:acceptanceCriteria",
          "@type": "PropertyValue",
          "name": "Acceptance Criteria",
          "description": "Conditions for feature completion",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:AcceptanceCriterion"
          }
        },
        {
          "@id": "efs:hasStory",
          "@type": "PropertyValue",
          "name": "Has Stories",
          "description": "User stories implementing this feature",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:UserStory"
          }
        },
        {
          "@id": "efs:enablesCapability",
          "@type": "PropertyValue",
          "name": "Enables Capability",
          "description": "Business capability this feature supports",
          "valueReference": "efs:Capability"
        },
        {
          "@id": "efs:technicalEnablers",
          "@type": "PropertyValue",
          "name": "Technical Enablers",
          "description": "Technical work required to support this feature",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:Enabler"
          }
        }
      ]
    },

    {
      "@id": "efs:UserStory",
      "@type": "Class",
      "name": "User Story",
      "description": "User-centric description of functionality from specific persona perspective",
      "rdfs:subClassOf": "efs:BacklogItem",
      "skos:broader": "schema:HowTo",
      "efs:moduleBinding": ["efs:BacklogManagement", "efs:UserExperience"],
      "efs:integrationPoint": {
        "efs:Feature": "efs:belongsToFeature",
        "efs:Persona": "efs:forPersona",
        "pmf:CustomerSegment": "efs:targetsSegment"
      },
      "efs:properties": [
        {
          "@id": "efs:asA",
          "@type": "PropertyValue",
          "name": "As A",
          "description": "The persona or role requesting this functionality",
          "valueReference": "efs:Persona"
        },
        {
          "@id": "efs:iWant",
          "@type": "PropertyValue",
          "name": "I Want",
          "description": "The desired functionality or capability",
          "valueReference": "xsd:string"
        },
        {
          "@id": "efs:soThat",
          "@type": "PropertyValue",
          "name": "So That",
          "description": "The benefit or value to be achieved",
          "valueReference": "xsd:string"
        },
        {
          "@id": "efs:storyAcceptanceCriteria",
          "@type": "PropertyValue",
          "name": "Acceptance Criteria",
          "description": "Specific conditions for story completion (Given-When-Then)",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:AcceptanceCriterion"
          }
        },
        {
          "@id": "efs:storyPoints",
          "@type": "QuantitativeValue",
          "name": "Story Points",
          "description": "Relative effort estimate",
          "unitText": "points"
        },
        {
          "@id": "efs:hasTasks",
          "@type": "PropertyValue",
          "name": "Has Tasks",
          "description": "Implementation tasks for this story",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:Task"
          }
        }
      ]
    },

    {
      "@id": "efs:Enabler",
      "@type": "Class",
      "name": "Enabler",
      "description": "Technical or architectural work supporting feature delivery without direct user value",
      "rdfs:subClassOf": "efs:BacklogItem",
      "efs:moduleBinding": ["efs:BacklogManagement", "efs:TechnicalDebt"],
      "efs:enablerTypes": [
        "efs:InfrastructureEnabler",
        "efs:ArchitecturalEnabler", 
        "efs:ExplorationEnabler",
        "efs:ComplianceEnabler"
      ],
      "efs:properties": [
        {
          "@id": "efs:enablerType",
          "@type": "PropertyValue",
          "name": "Enabler Type",
          "description": "Classification of enabler purpose",
          "valueReference": "efs:EnablerType"
        },
        {
          "@id": "efs:enablesFeatures",
          "@type": "PropertyValue",
          "name": "Enables Features",
          "description": "Features dependent on this enabler",
          "valueReference": {
            "@type": "ItemList",
            "itemListElement": "efs:Feature"
          }
        },
        {
          "@id": "efs:technicalDebtReduction",
          "@type": "QuantitativeValue",
          "name": "Technical Debt Reduction",
          "description": "Estimated debt reduction value"
        }
      ]
    },

    {
      "@id": "efs:Task",
