  "validationRules": [
    {
      "rule": "Process must have at least one phase",
      "passTests": ["test:process:app-scaffold-typical", "test:process:minimal-edge"],
      "failTests": ["test:process:invalid-missing-required"]
    },
    {
      "rule": "Phases cannot have circular dependencies",
      "passTests": ["test:phase:domain-definition", "test:phase:code-generation"],
      "failTests": ["test:phase:invalid-circular"]
    },
    {
      "rule": "Artifacts must be produced by at least one phase",
      "passTests": ["test:artifact:ontology-config", "test:artifact:typescript-types"],
      "failTests": ["test:artifact:invalid-no-producer"]
    }
  ]
