{
  "@context": {
    "@version": 1.1,
    "@base": "https://baiv.ai/ontology/process/mvp1.2/",
    "schema": "https://schema.org/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "baiv": "https://baiv.ai/ontology/process/",
    "pmf": "https://baiv.ai/ontology/pmf/",
    "okr": "https://baiv.ai/ontology/okr/",
    "vsom": "https://baiv.ai/ontology/vsom/"
  },
  "@graph": [
    {
      "@id": "baiv:BAIVMVP12ProcessOntology",
      "@type": "owl:Ontology",
      "rdfs:label": "BAIV MVP1.2 Process Ontology",
      "rdfs:comment": "Operational process ontology for Be AI Visible MVP1.2 customer acquisition and PMF validation, distinct from strategic frameworks",
      "schema:version": "1.2",
      "schema:dateCreated": "2025-09-21",
      "schema:creator": "BAIV Process Architecture Team",
      "owl:imports": [
        "https://baiv.ai/ontology/pmf/",
        "https://baiv.ai/ontology/vsom/",
        "https://schema.org/"
      ]
    },

    {
      "@id": "baiv:ProcessType",
      "@type": "owl:Class",
      "rdfs:label": "Process Type",
      "rdfs:comment": "Classification of operational processes in BAIV MVP1.2",
      "baiv:processCategories": [
        "OngoingProcess",
        "ProjectProcess", 
        "CampaignProcess",
        "CycleProcess"
      ]
    },

    {
      "@id": "baiv:OngoingProcess",
      "@type": "owl:Class",
      "rdfs:label": "Ongoing Process",
      "rdfs:comment": "Continuous operational processes with regular cycles and monitoring",
      "rdfs:subClassOf": "baiv:ProcessType",
      "baiv:characteristics": {
        "frequency": "continuous",
        "lifecycle": "indefinite",
        "ownership": "functional_team",
        "governance": "regular_reviews"
      },
      "baiv:examples": [
        "Customer acquisition pipeline",
        "Content production workflow",
        "AI visibility monitoring",
        "Lead nurturing automation"
      ]
    },

    {
      "@id": "baiv:ProjectProcess",
      "@type": "owl:Class", 
      "rdfs:label": "Project Process",
      "rdfs:comment": "One-time processes with defined start, end, and deliverables",
      "rdfs:subClassOf": "baiv:ProcessType",
      "baiv:characteristics": {
        "frequency": "one-time",
        "lifecycle": "defined_duration",
        "ownership": "project_manager",
        "governance": "milestone_gates"
      },
      "baiv:examples": [
        "MVP1.2 implementation",
        "P0-P16 agent deployment",
        "Ontology framework development",
        "PMF validation study"
      ]
    },

    {
      "@id": "baiv:CampaignProcess",
      "@type": "owl:Class",
      "rdfs:label": "Campaign Process", 
      "rdfs:comment": "Marketing and outreach campaigns with specific targets and timeframes",
      "rdfs:subClassOf": "baiv:ProcessType",
      "baiv:characteristics": {
        "frequency": "episodic",
        "lifecycle": "campaign_duration",
        "ownership": "marketing_team",
        "governance": "performance_tracking"
      },
      "baiv:examples": [
        "Batch targeting campaigns",
        "Industry-specific outreach",
        "Product launch campaigns",
        "BAIV awareness campaigns"
      ]
    },

    {
      "@id": "baiv:CycleProcess",
      "@type": "owl:Class",
      "rdfs:label": "Cycle Process",
      "rdfs:comment": "Recurring processes aligned with business cycles (quarterly, annual)",
      "rdfs:subClassOf": "baiv:ProcessType",
      "baiv:characteristics": {
        "frequency": "cyclical",
        "lifecycle": "cycle_aligned",
        "ownership": "senior_management",
        "governance": "cycle_reviews"
      },
      "baiv:examples": [
        "Quarterly OKR planning",
        "Annual strategy review",
        "PMF assessment cycles",
        "Customer retention reviews"
      ]
    },

    {
      "@id": "baiv:CustomerAcquisitionProcess",
      "@type": "owl:Class",
      "rdfs:label": "Customer Acquisition Process",
      "rdfs:comment": "End-to-end process for acquiring and onboarding new customers using BAIV methodology",
      "rdfs:subClassOf": "baiv:OngoingProcess",
      "baiv:processFlow": [
        "TargetIdentification",
        "BatchSegmentation", 
        "PMFValidation",
        "BAIVProcessExecution",
        "CustomerOnboarding",
        "ValueRealization",
        "RetentionOptimization"
      ],
      "baiv:dataInputs": {
        "targetList": "schema:Dataset",
        "industrySegmentation": "baiv:IndustryClassification",
        "contactData": "schema:ContactPoint",
        "behavioralData": "schema:UserInteraction"
      }
    },

    {
      "@id": "baiv:BatchProcessingFramework",
      "@type": "owl:Class",
