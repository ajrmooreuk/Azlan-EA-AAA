      "rdfs:subClassOf": "baiv:CycleProcess",
      "baiv:okrLevels": {
        "Strategic": {
          "owner": "SeniorManagement",
          "cycle": "Annual",
          "focus": "Vision and market positioning"
        },
        "Functional": {
          "owner": "FunctionHead",
          "cycle": "Quarterly", 
          "focus": "Operational excellence"
        },
        "Tactical": {
          "owner": "TeamLead",
          "cycle": "Monthly",
          "focus": "Process execution"
        },
        "Individual": {
          "owner": "TeamMember",
          "cycle": "Weekly",
          "focus": "Task completion"
        }
      }
    },

    {
      "@id": "baiv:ProcessOwnership",
      "@type": "owl:Class",
      "rdfs:label": "Process Ownership",
      "rdfs:comment": "Ownership and accountability framework for BAIV processes",
      "rdfs:subClassOf": "schema:Role",
      "baiv:ownershipTypes": {
        "ProcessOwner": {
          "responsibility": "End-to-end process accountability",
          "authority": "Process design and optimization",
          "accountability": "Process performance outcomes"
        },
        "ProcessManager": {
          "responsibility": "Day-to-day process execution",
          "authority": "Resource allocation within limits",
          "accountability": "Process efficiency metrics"
        },
        "ProcessParticipant": {
          "responsibility": "Task execution and feedback",
          "authority": "Local optimization decisions",
          "accountability": "Task quality and timeliness"
        }
      }
    },

    {
      "@id": "baiv:ProcessMetrics",
      "@type": "owl:Class",
      "rdfs:label": "Process Metrics",
      "rdfs:comment": "Measurement framework for BAIV operational processes",
      "rdfs:subClassOf": "schema:PropertyValue",
      "baiv:metricCategories": {
        "EfficiencyMetrics": [
          "ProcessCycleTime",
          "ResourceUtilization",
          "AutomationRatio",
          "ErrorRate"
        ],
        "EffectivenessMetrics": [
          "CustomerSatisfaction",
          "BusinessValueCreated",
          "QualityScore",
          "OutcomeAchievement"
        ],
        "StrategicMetrics": [
          "OKRAlignment",
          "StrategicContribution",
          "MarketImpact",
          "CompetitiveAdvantage"
        ]
      }
    },

    {
      "@id": "baiv:ProcessGovernance",
      "@type": "owl:Class",
      "rdfs:label": "Process Governance",
      "rdfs:comment": "Governance framework ensuring process alignment with strategic objectives",
      "rdfs:subClassOf": "schema:GovernmentOrganization",
      "baiv:governanceStructure": {
        "ProcessSteeringCommittee": {
          "composition": "Senior stakeholders",
          "frequency": "Quarterly",
          "purpose": "Strategic alignment and resource allocation"
        },
        "ProcessReviewBoard": {
          "composition": "Process owners and managers",
          "frequency": "Monthly", 
          "purpose": "Performance review and optimization"
        },
        "ProcessWorkingGroup": {
          "composition": "Process participants and experts",
          "frequency": "Weekly",
          "purpose": "Operational execution and improvement"
        }
      }
    },

    {
