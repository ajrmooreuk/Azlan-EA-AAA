        {"name": "esgReporting", "type": "xsd:boolean", "required": false, "description": "ESG reporting in place"},
        {"name": "esgRating", "type": "xsd:string", "required": false, "description": "External ESG rating if available"}
      ]
    },
    {
      "@id": "mat:PartnershipEcosystem",
      "@type": "rdfs:Class",
      "name": "PartnershipEcosystem",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Partnership and ecosystem engagement maturity.",
