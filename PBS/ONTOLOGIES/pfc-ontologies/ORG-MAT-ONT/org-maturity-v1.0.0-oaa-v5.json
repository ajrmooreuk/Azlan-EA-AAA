        {"name": "agileAdoption", "type": "xsd:string", "required": false, "enum": ["None", "Teams", "Departments", "Organization-wide", "Business Agility"], "description": "Agile adoption level"},
        {"name": "transformationHistory", "type": "xsd:string", "required": false, "enum": ["None", "Failed", "Mixed", "Successful", "Multiple Successes"], "description": "Transformation track record"},
        {"name": "changeManagement", "type": "xsd:boolean", "required": false, "description": "Formal change management practice"},
        {"name": "adaptationSpeed", "type": "xsd:string", "required": false, "enum": ["Slow", "Moderate", "Fast", "Very Fast"], "description": "Speed of adaptation"}
      ]
    }
  ],

