  "businessRules": [
    {
      "@id": "mat:BR-MAT-001",
      "name": "Profile Requires Dimension Scores",
      "description": "Every maturity profile must have at least one dimension score",
      "condition": "IF a MaturityProfile is created",
      "action": "THEN it MUST have at least one hasDimensionScore relationship",
      "severity": "error",
      "appliesTo": ["mat:MaturityProfile"]
    },
    {
      "@id": "mat:BR-MAT-002",
      "name": "Dimension Score Range",
      "description": "Numeric scores must be within valid range",
      "condition": "IF a DimensionScore has numericScore",
      "action": "THEN numericScore MUST be between 1.0 and 5.0 inclusive",
      "severity": "error",
      "appliesTo": ["mat:DimensionScore"]
    },
    {
      "@id": "mat:BR-MAT-003",
      "name": "Weight Range",
      "description": "Dimension weights must be valid percentages",
      "condition": "IF a DimensionScore has weight",
      "action": "THEN weight MUST be between 0.0 and 1.0 inclusive",
      "severity": "error",
      "appliesTo": ["mat:DimensionScore"]
    },
    {
      "@id": "mat:BR-MAT-004",
      "name": "Assessment Date Required",
      "description": "All profiles must have assessment date",
      "condition": "IF a MaturityProfile is created",
      "action": "THEN assessmentDate MUST be specified",
      "severity": "error",
      "appliesTo": ["mat:MaturityProfile"]
    },
    {
      "@id": "mat:BR-MAT-005",
      "name": "Level Score Consistency",
      "description": "Maturity level should align with numeric score",
      "condition": "IF a DimensionScore has both maturityLevel and numericScore",
      "action": "THEN maturityLevel SHOULD correspond to score range (Initial: 1.0-1.9, Developing: 2.0-2.9, Defined: 3.0-3.9, Managed: 4.0-4.9, Optimized: 5.0)",
      "severity": "warning",
      "appliesTo": ["mat:DimensionScore"]
    },
    {
      "@id": "mat:BR-MAT-006",
      "name": "Core Dimensions Recommended",
      "description": "Profiles should include core dimensional assessments",
      "condition": "IF a MaturityProfile is created for comprehensive assessment",
      "action": "THEN it SHOULD include at minimum: SizeProfile, SectorProfile, GeographyProfile, and at least 3 maturity dimensions",
      "severity": "info",
      "appliesTo": ["mat:MaturityProfile"]
    },
    {
      "@id": "mat:BR-MAT-007",
      "name": "Evidence for High Scores",
      "description": "High maturity claims should have supporting evidence",
      "condition": "IF a DimensionScore has numericScore >= 4.0",
      "action": "THEN evidence SHOULD be provided",
      "severity": "warning",
      "appliesTo": ["mat:DimensionScore"]
    },
    {
      "@id": "mat:BR-MAT-008",
      "name": "NPS Score Range",
      "description": "NPS scores must be within valid range",
      "condition": "IF CustomerExperienceMaturity has npsScore",
      "action": "THEN npsScore MUST be between -100 and 100 inclusive",
      "severity": "error",
      "appliesTo": ["mat:CustomerExperienceMaturity"]
    }
  ],

