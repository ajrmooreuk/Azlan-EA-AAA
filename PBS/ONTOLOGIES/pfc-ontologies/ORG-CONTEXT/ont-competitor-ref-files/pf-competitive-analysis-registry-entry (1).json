    "ontologyMetadata": {
      "identifier": "pf:ontology:competitive-analysis-v1",
      "name": "PF Competitive Analysis Ontology",
      "version": "1.0.0",
      "status": "active",
      "frameworkType": "Platform Framework (Domain-Agnostic)",
      "domain": ["Competitive Intelligence", "Strategic Analysis", "Market Research"],
      "applicableSectors": ["Technology", "Healthcare", "Finance", "E-commerce", "Manufacturing", "Services"],
      "registrationDate": "2025-10-08T15:45:00Z",
      "lastModified": "2025-10-08T15:45:00Z",
      "registeredBy": "Platform Framework Architecture Team",
      "businessOwner": "Chief Strategy Officer (Configurable)",
      "technicalOwner": "Lead Ontology Architect"
    },
    
    "configurationFramework": {
      "configurationType": "Template-Based with Variable Substitution",
      "requiredConfigurationVariables": {
        "TARGET_ORG_ID": {
          "description": "Unique identifier for the target organization",
          "type": "string",
          "pattern": "org:[organization-name]",
          "example": "org:techstart-ai"
        },
        "TARGET_ORG_NAME": {
          "description": "Display name of the target organization", 
          "type": "string",
          "example": "TechStart AI"
        },
        "MARKET_DOMAIN": {
          "description": "Primary market domain for competitive analysis",
          "type": "string",
          "examples": ["SaaS Integration", "Digital Payments", "Healthcare Technology", "E-commerce Platforms"]
        },
        "MARKET_SUBSECTOR": {
          "description": "Specific subsector within the market domain",
          "type": "string", 
          "examples": ["API Management", "B2B Payments", "Telemedicine", "Payment Gateways"]
        },
        "GEOGRAPHICAL_SCOPE": {
          "description": "Geographical boundaries for competitive analysis",
          "type": "string",
          "examples": ["North America", "Global", "EMEA", "United States"]
        },
        "PRODUCT_CATEGORY": {
          "description": "Primary product or service category",
          "type": "string",
          "examples": ["Integration Platform", "Payment Gateway", "Telehealth Platform", "E-commerce Platform"]
        }
      },
      "optionalConfigurationVariables": {
        "OKR_FRAMEWORK": {
          "description": "Reference to organizational OKR system",
          "type": "reference",
          "example": "okr:q3-2025-objectives"
        },
        "VALUE_PROPOSITION_ONTOLOGY": {
          "description": "Reference to VSOM (Value Proposition Ontology)",
          "type": "reference", 
          "example": "vsom:techstart-value-model"
        },
        "STRATEGIC_CONTEXT": {
          "description": "Business strategy context reference",
          "type": "reference",
          "example": "strategy:developer-first-growth"
        },
        "COMPETITIVE_TIMEFRAME": {
          "description": "Time horizon for competitive analysis",
          "type": "string",
          "default": "18 months",
          "examples": ["6 months", "12 months", "18 months", "3 years"]
        }
      },
      "competitorConfigurationPattern": {
        "DIRECT_COMPETITORS": {
          "description": "3-5 primary direct competitors (BR-001)",
          "type": "array[string]",
          "cardinality": "3..5",
          "example": ["Competitor A", "Competitor B", "Competitor C", "Competitor D"]
        },
        "INDIRECT_COMPETITORS": {
          "description": "Secondary competitive threats",
          "type": "array[string]",
          "cardinality": "0..*",
          "example": ["Indirect Competitor 1", "Indirect Competitor 2"]
        },
        "EMERGING_COMPETITORS": {
          "description": "Emerging competitive threats to monitor",
          "type": "array[string]",
          "cardinality": "0..*",
          "example": ["Startup X", "Stealth Company Y"]
        }
      }
    },
    
    "businessContext": {
      "primaryPurpose": "Enable systematic competitive analysis across any market domain and organizational context",
      "businessValue": [
        "Domain-agnostic competitive intelligence framework",
        "Rapid deployment across diverse market sectors", 
        "Strategic competitive positioning for any organization type",
        "Standardized competitive analysis methodology",
        "AI agent-enabled competitive insights automation"
      ],
      "targetStakeholders": [
        "Strategy teams across all industries",
        "Product managers in any domain", 
        "Marketing leaders in competitive markets",
        "Business development across sectors",
        "Executive leadership teams",
        "AI agents and competitive intelligence systems"
      ],
      "universalUseCases": [
        {
          "id": "UC-001",
          "name": "Direct Competitor Benchmarking",
          "description": "Analyze 3-5 primary competitors in [MARKET_DOMAIN]",
          "businessValue": "Focused competitive strategy for any market",
          "applicableTo": "All organizations with competitive markets"
        },
        {
          "id": "UC-002", 
          "name": "Market Leader Analysis",
          "description": "Profile market leaders in [MARKET_SUBSECTOR]",
          "businessValue": "Comprehensive market intelligence",
          "applicableTo": "Organizations entering or competing in established markets"
        },
        {
          "id": "UC-003",
          "name": "Blue Ocean Identification", 
          "description": "Identify uncontested opportunities in [MARKET_DOMAIN]",
          "businessValue": "Strategic differentiation and new market creation",
          "applicableTo": "Innovation-focused organizations across all sectors"
        },
        {
          "id": "UC-004",
          "name": "Strategic Objective Alignment",
          "description": "Align competitive analysis with [OKR_FRAMEWORK]",
          "businessValue": "Strategy-driven competitive intelligence",
          "applicableTo": "Organizations with structured strategic planning"
        }
      ]
    },
    
    "technicalSpecification": {
      "entityCount": 10,
      "relationshipCount": 12,
      "propertyCount": 95,
      "businessRuleCount": 5,
      "configurationVariableCount": 10,
      
      "coreEntities": [
        {
          "name": "TargetOrganization",
          "schemaOrgBase": "Organization",
          "purpose": "Configurable organization requiring competitive analysis in [MARKET_DOMAIN]",
          "keyProperties": ["organizationMaturity", "domainReadiness", "directCompetitors", "okrFramework"],
          "configurability": "Fully configurable for any organization type"
        },
        {
          "name": "CompetitorOrganization", 
          "schemaOrgBase": "Organization",
          "purpose": "Competitors in [MARKET_DOMAIN] within [GEOGRAPHICAL_SCOPE]",
          "keyProperties": ["competitorType", "threatLevel", "domainCapabilities"],
          "configurability": "Adaptable to any competitive landscape"
        },
        {
          "name": "MarketOffering",
          "schemaOrgBase": "Product", 
          "purpose": "Products/services in [PRODUCT_CATEGORY] within [MARKET_DOMAIN]",
          "keyProperties": ["domainFeatures", "competitiveDifferentiators"],
          "configurability": "Flexible for any product or service type"
        },
        {
          "name": "MarketSegment",
          "schemaOrgBase": "Intangible",
          "purpose": "Market segments within [MARKET_DOMAIN] and [GEOGRAPHICAL_SCOPE]", 
          "keyProperties": ["competitiveIntensity", "regulatoryFactors", "technologyTrends"],
          "configurability": "Applicable to any market segmentation"
        },
        {
          "name": "StrategicObjective",
          "schemaOrgBase": "Intangible",
          "purpose": "OKRs driving competitive strategy, linked to [OKR_FRAMEWORK]",
          "keyProperties": ["keyResults", "competitiveRelevance", "strategicPriority"],
          "configurability": "Integrates with any OKR or strategic planning system"
        },
        {
          "name": "ValueProposition",
          "schemaOrgBase": "Intangible",
          "purpose": "Value delivery in [MARKET_DOMAIN], connected to [VALUE_PROPOSITION_ONTOLOGY]",
          "keyProperties": ["coreValue", "differentiators", "vsomReference"],
          "configurability": "Links to any VSOM or value framework"
        },
        {
          "name": "BlueOceanOpportunity",
          "schemaOrgBase": "Intangible",
          "purpose": "Uncontested opportunities in [MARKET_DOMAIN]",
          "keyProperties": ["competitiveBarriers", "marketEducationNeeded"],
          "configurability": "Identifies opportunities across any market"
        }
      ],
      
      "configurableRelationships": [
        "TargetOrganization --direct_competitor(3-5)--> CompetitorOrganization",
        "TargetOrganization --pursues_objective--> StrategicObjective", 
        "CompetitorOrganization --operates_in_segment--> MarketSegment",
        "ValueProposition --references_vsom--> [VALUE_PROPOSITION_ONTOLOGY]",
        "CompetitiveAnalysis --informs_objective--> StrategicObjective"
      ],
      
      "universalBusinessRules": [
        "BR-001: TargetOrganization must identify 3-5 direct competitors in [MARKET_DOMAIN]",
        "BR-002: BlueOceanOpportunity must have competitiveBarriers='Low'|'None'", 
        "BR-003: CompetitorOrganization must operate in ≥1 MarketSegment in [MARKET_DOMAIN]",
        "BR-004: Direct competitors must have defined threatLevel",
        "BR-005: StrategicObjective must have ≥1 measurable keyResult"
      ]
    },
    
