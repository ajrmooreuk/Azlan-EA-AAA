  "schemaOrgAlignment": {
    "baseTypes": [
      "CreativeWork",
      "DefinedTerm",
      "PropertyValue"
    ],
    "rationale": "Design tokens modelled as DefinedTerm instances within a DefinedTermSet; components as CreativeWork with encoding metadata",
    "extensions": [
      "DesignToken",
      "TokenTier",
      "ComponentSpec",
      "DesignPrimitive",
      "SemanticToken",
      "ComponentToken"
    ]
  },
  "tokenArchitecture": {
    "@type": "ds:TokenArchitecture",
    "description": "Three-tier cascade: Primitives (raw values, NEVER in component code) → Semantics (intent-based, general styling) → Components (specific component overrides)",
    "cascade": [
      {
        "tier": 1,
        "name": "Primitives",
        "prefix": "--prim-",
        "usage": "INTERNAL ONLY — referenced by semantic and component tokens. Never used directly in component code.",
        "cssScope": ":root"
      },
      {
        "tier": 2,
        "name": "Semantics",
        "prefix": "--sem-",
        "usage": "Intent-based tokens describing WHAT the value does. Use for general styling when no component token exists.",
        "cssScope": ":root"
      },
      {
        "tier": 3,
        "name": "Components",
        "prefix": "--comp-",
        "usage": "Component-specific tokens. Use FIRST when generating code for a known component.",
        "cssScope": "[data-component] or :root"
      }
    ],
    "criticalRules": [
      "NEVER hardcode colors — always use var(--token-name)",
      "NEVER use primitive tokens in component code",
      "USE semantic tokens for general styling",
      "USE component tokens FIRST when they exist for that component",
      "CASCADE: Component tokens → Semantic tokens → (Primitives are internal only)"
    ]
  },
