    "dateCreated": "2026-02-02",
    "dateModified": "2026-02-02",
    "creator": "PF-Core Platform Team",
    "domain": "Enterprise Architecture / Design System / Figma-to-Code",
    "purpose": "Provide design token ontology for F2C pipeline ensuring brand-consistent code generation from Figma Make. Part of PF-Instance enterprise architecture for Partners& digital transformation.",
    "frameworkType": "Platform Framework (Domain-Agnostic DS Layer)",
    "targetOrganization": "Partners& Limited",
    "dependencies": [
      "pfc:ontology:design-system-f2c-v1",
      "pfi:ontology:org-context-partners-and-v1"
    ],
    "toolchain": {
      "designTool": "Figma",
      "codeGenerator": "Figma Make",
      "framework": "Next.js 14+",
      "uiLibrary": "shadcn/ui",
      "cssStrategy": "CSS Custom Properties + Tailwind",
      "tokenBridge": "F2C Token Specification Document"
