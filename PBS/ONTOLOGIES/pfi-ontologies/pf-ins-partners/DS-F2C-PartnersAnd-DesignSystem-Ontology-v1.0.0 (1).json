  "figmaMakeInstructions": {
    "@type": "ds:FigmaMakeSpec",
    "description": "Compact specification for pasting into Figma Make project instructions to achieve token-aware code generation",
    "usageInstructions": [
      "Copy the tokenArchitecture.criticalRules and paste into Figma Make Project Instructions",
      "Include the full semantic and component token sections relevant to the components being generated",
      "Figma Make will then emit var(--comp-*) and var(--sem-*) references instead of hardcoded values",
      "The generated CSS custom properties can be defined in globals.css or a theme provider"
    ],
    "codePatternExample": {
      "component": "ServiceCard",
      "correctCode": "<Card className=\"group\" style={{background: 'var(--comp-svc-card-bg)', borderRadius: 'var(--comp-svc-card-radius)'}}>...",
      "incorrectCode": "<Card className=\"bg-white rounded-xl\">...",
      "explanation": "Always use CSS custom property references via var(), never Tailwind utility classes for brand values"
    }
  },
