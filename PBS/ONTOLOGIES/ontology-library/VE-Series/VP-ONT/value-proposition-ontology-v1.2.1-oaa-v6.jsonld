{
  "@context": {
    "@vocab": "https://schema.org/",
    "oaa": "https://oaa-ontology.org/v6/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "vp": "https://oaa-ontology.org/v6/value-proposition/",
    "vsom": "https://oaa-ontology.org/v6/vsom/",
    "org": "https://oaa-ontology.org/v6/organization/",
    "efs": "https://oaa-ontology.org/v6/efs/",
    "pmf": "https://oaa-ontology.org/v6/pmf/",
    "pf": "https://platform-foundation.io/ontology/v1/",
    "raci": "https://platform-foundation.io/ontology/v1/raci/"
  },
  "@id": "https://oaa-ontology.org/v6/vp/schema",
  "@type": "owl:Ontology",
  "name": "Value Proposition Ontology",
  "alternateName": "VP-ONT",
  "description": "Comprehensive ontology for developing, validating, and articulating value propositions. Links VSOM strategic layer to EFS execution layer through ICP-driven problem-solution mapping. Scoped by OrganizationContext for PFI-instance deployment. v1.2.1 upgrades to OAA v6.1.0 with strict IF-THEN business rules.",
  "oaa:schemaVersion": "6.1.0",
  "oaa:moduleVersion": "1.2.1",
  "oaa:previousVersion": "1.2.0",
  "oaa:domain": "Value Engineering, Product Strategy, Customer Value, Organizational Accountability",
  "oaa:status": "Production",
  "dateCreated": "2025-10-08",
  "dateModified": "2026-02-02",
  "creator": {
    "@type": "Organization",
    "name": "PFC Core Capability Framework"
  },
  "oaa:imports": [
    {
      "@id": "vsom:schema",
      "name": "VSOM-ONT",
      "version": "2.1.0",
      "importedEntities": [
        "vsom:VSOMFramework",
        "vsom:StrategyComponent",
        "vsom:ObjectivesComponent",
        "vsom:Outcome",
        "vsom:BusinessImpact"
      ],
      "importedRelationships": [
        "vsom:hasStrategicContext",
        "vsom:drivesOutcome"
      ]
    },
    {
      "@id": "org:schema",
      "name": "ORG-ONT",
      "version": "2.1.0",
      "importedEntities": [
        "org:Organization",
        "org:OrganizationContext"
      ],
      "importedRelationships": [
        "org:hasContext"
      ]
    },
    {
      "@id": "efs:schema",
      "name": "EFS-ONT",
      "version": "3.0.0",
      "importedEntities": [
        "efs:Epic",
        "efs:Feature",
        "efs:UserStory",
        "efs:Persona",
        "efs:AcceptanceCriterion"
      ],
      "importedRelationships": [
        "efs:hasFeature",
        "efs:hasStory"
      ]
    },
    {
      "@id": "pf:ontology:roles-raci-rbac-v3.0.0",
      "name": "RRR-ONT",
      "version": "3.0.0",
      "importedEntities": [
        "pf:ExecutiveRole",
        "pf:FunctionalRole",
        "pf:RoleAssignment",
        "raci:RACIAssignment"
      ],
      "importedRelationships": [
        "pf:reportsTo",
        "pf:collaboratesWith",
        "raci:hasRACIAssignment"
      ],
      "integrationPurpose": "Enable hierarchical ICP structure mirroring organizational roles, problem roll-up by seniority level, and RACI accountability for VP activities"
    }
  ],
  "oaa:joinPatterns": [
    {
      "patternId": "JP-VP-001",
      "name": "VSOM-to-VP Strategic Alignment",
      "description": "Value Proposition aligns to VSOM strategic objectives",
      "joinPath": "vsom:ObjectivesComponent → vp:alignsToObjective ← vp:ValueProposition",
      "useCase": "Strategic objective drives value proposition definition"
    },
    {
      "patternId": "JP-VP-002",
      "name": "VP-to-EFS Execution Mapping",
      "description": "Value Proposition problems map to EFS execution items",
      "joinPath": "vp:Problem → vp:mapsToEpic → efs:Epic → efs:hasFeature → efs:Feature",
      "useCase": "Problem severity determines Epic/Feature decomposition"
    },
    {
      "patternId": "JP-VP-003",
      "name": "ICP-to-Persona Bridge",
      "description": "Ideal Customer Profile maps to EFS Personas for story writing",
      "joinPath": "vp:IdealCustomerProfile → vp:manifestsAsPersona → efs:Persona",
      "useCase": "ICP defines persona characteristics for user stories"
    },
    {
      "patternId": "JP-VP-004",
      "name": "Org-Context Scoping",
      "description": "Value Proposition scoped by OrganizationContext for PFI-instance",
      "joinPath": "org:OrganizationContext → vp:hasValueProposition → vp:ValueProposition",
      "useCase": "Each PFI-instance has org-specific value propositions"
    },
    {
      "patternId": "JP-VP-005",
      "name": "Benefit-to-Story Mapping",
      "description": "VP Benefits map to UserStory 'So That' outcomes",
      "joinPath": "vp:Benefit → vp:realizesInStory → efs:UserStory.soThat",
      "useCase": "Benefits define story acceptance outcomes"
    },
    {
      "patternId": "JP-VP-008",
      "name": "RRR-to-ICP Role Bridge",
      "description": "Organizational roles link to Role-Based ICPs for hierarchical customer profiling",
      "joinPath": "pf:ExecutiveRole|pf:FunctionalRole → vp:roleRef ← vp:RoleBasedICP → vp:icpReportsTo → vp:RoleBasedICP",
      "useCase": "Navigate from organizational role to ICP and up/down hierarchy",
      "newInVersion": "1.2.0"
    },
    {
      "patternId": "JP-VP-009",
      "name": "Problem Roll-Up Chain",
      "description": "Operational problems aggregate to tactical, then to strategic",
      "joinPath": "vp:Problem[Operational] → vp:problemRollsUpTo → vp:Problem[Tactical] → vp:problemRollsUpTo → vp:Problem[Strategic]",
      "useCase": "Trace root cause from strategic problem to operational details",
      "newInVersion": "1.2.0"
    },
    {
      "patternId": "JP-VP-010",
      "name": "RACI-VP Activity Chain",
      "description": "RACI assignments for VP activities link to roles and their ICPs",
      "joinPath": "vp:VPRACIAssignment → raci:role → pf:FunctionalRole → vp:roleRef ← vp:RoleBasedICP → vp:icpHasProblem → vp:Problem",
      "useCase": "Determine who is responsible for resolving specific problems by role",
      "newInVersion": "1.2.0"
    },
    {
      "patternId": "JP-VP-011",
      "name": "ICP Hierarchy to EFS Persona",
      "description": "Multiple operational ICPs can share tactical-level persona for story writing",
      "joinPath": "vp:RoleBasedICP[Operational] → vp:icpReportsTo → vp:RoleBasedICP[Tactical] → vp:manifestsAsPersona → efs:Persona",
      "useCase": "Persona at tactical level represents multiple operational role ICPs",
      "newInVersion": "1.2.0"
    }
  ],
  "entities": [
    {
      "@id": "vp:ValueProposition",
      "@type": "rdfs:Class",
      "name": "ValueProposition",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Complete articulation of how solution creates value for target customers by addressing their problems better than alternatives. Primary statement format: For [ICP] who [NEED], our [SOLUTION] provides [BENEFITS] unlike [ALTERNATIVES] because [DIFFERENTIATORS]. Scoped by OrganizationContext.",
      "oaa:properties": [
        {
          "name": "vpId",
          "type": "xsd:string",
          "required": true,
          "pattern": "vp:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "name",
          "type": "xsd:string",
          "required": true,
          "schemaOrgMapping": "schema:name"
        },
        {
          "name": "primaryStatement",
          "type": "xsd:string",
          "required": true,
          "minLength": 50,
          "description": "Core value proposition statement"
        },
        {
          "name": "alternativeStatement",
          "type": "xsd:string",
          "required": false,
          "description": "Alternative articulation format"
        },
        {
          "name": "keyBenefits",
          "type": "xsd:string",
          "required": true,
          "description": "Summary of key benefits delivered"
        },
        {
          "name": "uniqueDifferentiators",
          "type": "xsd:string",
          "required": true,
          "description": "What makes this solution unique"
        },
        {
          "name": "organizationContextRef",
          "type": "xsd:string",
          "required": true,
          "description": "Reference to org:OrganizationContext for PFI-instance scoping"
        },
        {
          "name": "status",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Draft",
            "Validated",
            "Active",
            "Deprecated"
          ]
        },
        {
          "name": "validationDate",
          "type": "xsd:date",
          "required": false,
          "description": "Date VP was validated with customers"
        }
      ]
    },
    {
      "@id": "vp:IdealCustomerProfile",
      "@type": "rdfs:Class",
      "name": "IdealCustomerProfile",
      "rdfs:subClassOf": "schema:Audience",
      "description": "Detailed specification of ideal target customer including demographics, psychographics, firmographics, and willingness to pay. Maps to EFS Personas for execution. ICP at org level, Persona at product/feature level.",
      "oaa:properties": [
        {
          "name": "icpId",
          "type": "xsd:string",
          "required": true,
          "pattern": "icp:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "name",
          "type": "xsd:string",
          "required": true,
          "schemaOrgMapping": "schema:name"
        },
        {
          "name": "customerType",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "B2B",
            "B2C",
            "B2B2C"
          ]
        },
        {
          "name": "segmentDescription",
          "type": "xsd:string",
          "required": true,
          "minLength": 20,
          "description": "Detailed segment characteristics"
        },
        {
          "name": "demographics",
          "type": "xsd:string",
          "required": false,
          "description": "Industry, size, location, revenue range"
        },
        {
          "name": "firmographics",
          "type": "xsd:string",
          "required": false,
          "description": "Company structure, tech stack, growth stage"
        },
        {
          "name": "psychographics",
          "type": "xsd:string",
          "required": false,
          "description": "Values, goals, behaviors, decision patterns"
        },
        {
          "name": "willingnessToPayIndicator",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "High",
            "Medium",
            "Low"
          ]
        },
        {
          "name": "marketSize",
          "type": "xsd:decimal",
          "required": false,
          "description": "Addressable market size for this ICP"
        },
        {
          "name": "acquisitionCost",
          "type": "xsd:decimal",
          "required": false,
          "description": "Estimated customer acquisition cost"
        },
        {
          "name": "lifetimeValue",
          "type": "xsd:decimal",
          "required": false,
          "description": "Estimated customer lifetime value"
        }
      ]
    },
    {
      "@id": "vp:RoleBasedICP",
      "@type": "rdfs:Class",
      "name": "RoleBasedICP",
      "rdfs:subClassOf": "vp:IdealCustomerProfile",
      "description": "ICP specialized for a specific organizational role from RRR ontology, enabling role-level problem decomposition and hierarchical roll-up to strategic ICP. Links organizational structure to customer profiling.",
      "newInVersion": "1.2.0",
      "oaa:properties": [
        {
          "name": "roleRef",
          "type": "xsd:string",
          "required": true,
          "description": "Reference to pf:ExecutiveRole or pf:FunctionalRole from RRR ontology"
        },
        {
          "name": "parentICP",
          "type": "xsd:string",
          "required": false,
          "description": "Reference to parent ICP in hierarchy (for roll-up)"
        },
        {
          "name": "seniorityLevel",
          "type": "xsd:integer",
          "required": true,
          "minimum": 1,
          "maximum": 10,
          "description": "Mirrors RRR seniorityLevel: 1=C-Suite, 2=VP, 3=Director, 4=Manager, 5=Individual Contributor"
        },
        {
          "name": "functionScope",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Strategic",
            "Tactical",
            "Operational"
          ],
          "description": "Scope of problems this role addresses"
        },
        {
          "name": "decisionAuthority",
          "type": "xsd:string",
          "required": false,
          "enum": [
            "Final",
            "Recommender",
            "Influencer",
            "User"
          ],
          "description": "Decision-making authority level"
        },
        {
          "name": "budgetRange",
          "type": "xsd:string",
          "required": false,
          "description": "Budget authority range for purchase decisions"
        },
        {
          "name": "problemScopeFilter",
          "type": "xsd:string",
          "required": false,
          "description": "Filter for which problems apply at this role level"
        }
      ]
    },
    {
      "@id": "vp:Problem",
      "@type": "rdfs:Class",
      "name": "Problem",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Challenge or unmet need experienced by ICP creating negative consequences. Problems with high severity map to EFS Epics; medium/low severity map to Features. Analyzed across functional, economic, emotional, social dimensions. v1.2.0 adds scopeLevel and ownerRole for hierarchical organization.",
      "oaa:properties": [
        {
          "name": "problemId",
          "type": "xsd:string",
          "required": true,
          "pattern": "prob:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "problemStatement",
          "type": "xsd:string",
          "required": true,
          "minLength": 30,
          "description": "[ICP] struggles with [CHALLENGE] when [CONTEXT], resulting in [CONSEQUENCES]"
        },
        {
          "name": "problemType",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Functional",
            "Economic",
            "Emotional",
            "Social"
          ]
        },
        {
          "name": "severity",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Critical",
            "Major",
            "Moderate",
            "Minor"
          ],
          "description": "Critical/Major → Epic; Moderate/Minor → Feature"
        },
        {
          "name": "frequency",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Constant",
            "Frequent",
            "Occasional",
            "Rare"
          ]
        },
        {
          "name": "isGrowing",
          "type": "xsd:boolean",
          "required": false,
          "description": "Whether problem is growing over time"
        },
        {
          "name": "marketSizeImpact",
          "type": "xsd:decimal",
          "required": false,
          "description": "TAM/SAM/SOM affected by this problem"
        },
        {
          "name": "scopeLevel",
          "type": "xsd:string",
          "required": false,
          "enum": [
            "Strategic",
            "Tactical",
            "Operational"
          ],
          "description": "Level in problem hierarchy (new in v1.2.0)",
          "newInVersion": "1.2.0"
        },
        {
          "name": "ownerRole",
          "type": "xsd:string",
          "required": false,
          "description": "Reference to role that owns this problem (new in v1.2.0)",
          "newInVersion": "1.2.0"
        }
      ]
    },
    {
      "@id": "vp:PainPoint",
      "@type": "rdfs:Class",
      "name": "PainPoint",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Specific source of difficulty within a Problem. Pain points drive feature prioritization. Dimensions: Functional (task blockers), Economic (cost impacts), Emotional (frustrations), Social (status/perception).",
      "oaa:properties": [
        {
          "name": "painId",
          "type": "xsd:string",
          "required": true,
          "pattern": "pain:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "painDescription",
          "type": "xsd:string",
          "required": true,
          "minLength": 20,
          "description": "Detailed pain point description"
        },
        {
          "name": "painDimension",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Functional",
            "Economic",
            "Emotional",
            "Social"
          ]
        },
        {
          "name": "customerImpact",
          "type": "xsd:string",
          "required": true,
          "description": "How this pain affects the customer"
        },
        {
          "name": "quantifiedImpact",
          "type": "xsd:decimal",
          "required": false,
          "description": "Quantified cost, time, or revenue impact"
        },
        {
          "name": "impactUnit",
          "type": "xsd:string",
          "required": false,
          "enum": [
            "Currency",
            "Hours",
            "Percentage",
            "Count"
          ]
        }
      ]
    },
    {
      "@id": "vp:Gain",
      "@type": "rdfs:Class",
      "name": "Gain",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Desired outcome or positive result ICP wants to achieve. Gains map to EFS Features. Represents the 'jobs to be done' perspective - what customer wants to accomplish.",
      "oaa:properties": [
        {
          "name": "gainId",
          "type": "xsd:string",
          "required": true,
          "pattern": "gain:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "gainDescription",
          "type": "xsd:string",
          "required": true,
          "minLength": 20,
          "description": "What the customer wants to achieve"
        },
        {
          "name": "gainType",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Required",
            "Expected",
            "Desired",
            "Unexpected"
          ]
        },
        {
          "name": "gainCategory",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Functional",
            "Social",
            "Emotional",
            "Economic"
          ]
        },
        {
          "name": "priority",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Critical",
            "High",
            "Medium",
            "Low"
          ]
        },
        {
          "name": "quantifiedValue",
          "type": "xsd:decimal",
          "required": false,
          "description": "Quantified value of achieving this gain"
        }
      ]
    },
    {
      "@id": "vp:Solution",
      "@type": "rdfs:Class",
      "name": "Solution",
      "rdfs:subClassOf": "schema:Product",
      "description": "Product, service, or approach addressing identified problems and delivering gains. Must address at least one Problem and deliver at least one Benefit with clear differentiators.",
      "oaa:properties": [
        {
          "name": "solutionId",
          "type": "xsd:string",
          "required": true,
          "pattern": "sol:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "name",
          "type": "xsd:string",
          "required": true,
          "schemaOrgMapping": "schema:name"
        },
        {
          "name": "description",
          "type": "xsd:string",
          "required": true,
          "minLength": 30,
          "schemaOrgMapping": "schema:description"
        },
        {
          "name": "coreFunctionality",
          "type": "xsd:string",
          "required": true,
          "description": "Primary capabilities of solution"
        },
        {
          "name": "deliveryMethod",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "SaaS",
            "OnPremise",
            "Hybrid",
            "Service",
            "Platform",
            "Marketplace"
          ]
        },
        {
          "name": "pricingModel",
          "type": "xsd:string",
          "required": false,
          "enum": [
            "Subscription",
            "Usage",
            "Freemium",
            "License",
            "Custom"
          ]
        },
        {
          "name": "timeToValue",
          "type": "xsd:duration",
          "required": false,
          "description": "Time for customer to realize value"
        }
      ]
    },
    {
      "@id": "vp:Benefit",
      "@type": "rdfs:Class",
      "name": "Benefit",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Positive outcome delivered to customers by solution. Benefits map to EFS UserStory 'So That' clause. Must mitigate at least one PainPoint. Quantifiable benefits preferred.",
      "oaa:properties": [
        {
          "name": "benefitId",
          "type": "xsd:string",
          "required": true,
          "pattern": "ben:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "benefitDescription",
          "type": "xsd:string",
          "required": true,
          "minLength": 20,
          "description": "What value is delivered"
        },
        {
          "name": "benefitType",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Quantifiable",
            "Qualitative"
          ]
        },
        {
          "name": "benefitCategory",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "CostReduction",
            "RevenueGrowth",
            "TimeSavings",
            "RiskReduction",
            "ExperienceImprovement",
            "StrategicAdvantage",
            "ComplianceAssurance"
          ]
        },
        {
          "name": "quantifiedValue",
          "type": "xsd:decimal",
          "required": false,
          "description": "Quantified benefit value"
        },
        {
          "name": "measurementMethod",
          "type": "xsd:string",
          "required": false,
          "description": "How benefit is measured"
        },
        {
          "name": "realizationTimeframe",
          "type": "xsd:duration",
          "required": false,
          "description": "Time to realize this benefit"
        }
      ]
    },
    {
      "@id": "vp:Differentiator",
      "@type": "rdfs:Class",
      "name": "Differentiator",
      "rdfs:subClassOf": "schema:Intangible",
      "description": "Unique aspect distinguishing solution from alternatives. Maps to EFS Capability. Assessed for defensibility and customer importance.",
      "oaa:properties": [
        {
          "name": "differentiatorId",
          "type": "xsd:string",
          "required": true,
          "pattern": "diff:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "differentiatorDescription",
          "type": "xsd:string",
          "required": true,
          "minLength": 20,
          "description": "What makes this unique"
        },
        {
          "name": "differentiatorType",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "Technology",
            "Approach",
            "Capability",
            "BusinessModel",
            "Experience",
            "Integration",
            "Data",
            "Network"
          ]
        },
        {
          "name": "competitiveAdvantage",
          "type": "xsd:string",
          "required": true,
          "description": "Specific advantage over alternatives"
        },
        {
          "name": "defensibility",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "High",
            "Medium",
            "Low"
          ],
          "description": "How difficult to copy"
        },
        {
          "name": "customerImportance",
          "type": "xsd:string",
          "required": true,
          "enum": [
            "High",
            "Medium",
            "Low"
          ],
          "description": "How important to target customers"
        }
      ]
    },
    {
      "@id": "vp:CompetitiveAlternative",
      "@type": "rdfs:Class",
      "name": "CompetitiveAlternative",
      "rdfs:subClassOf": "schema:Product",
      "description": "Existing product, service, or approach competing with proposed solution. Includes direct competitors, substitutes, and 'do nothing' option.",
      "oaa:properties": [
        {
          "name": "alternativeId",
          "type": "xsd:string",
          "required": true,
          "pattern": "alt:[a-z0-9-]+",
          "schemaOrgMapping": "schema:identifier"
        },
        {
          "name": "name",
          "type": "xsd:string",
          "required": true,
          "schemaOrgMapping": "schema:name"
