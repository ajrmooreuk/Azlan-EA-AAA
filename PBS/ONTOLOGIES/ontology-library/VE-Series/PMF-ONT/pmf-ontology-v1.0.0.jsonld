    "versionHistory": [
      {
        "version": "1.0.0",
        "date": "2026-02-07",
        "notes": "Initial OAA v5.0.0 compliant ontology generated from PMF-GTM agent specifications and registry entry definitions"
      }
    ]
  }
}
