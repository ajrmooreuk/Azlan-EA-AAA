{
  "@context": {
    "@vocab": "https://schema.org/",
    "gtm": "https://platformcore.io/ontology/gtm/",
    "efs": "https://platformcore.io/ontology/efs/",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@id": "gtm:GoToMarketOntology",
  "@type": "DefinedTermSet",
  "name": "Go-To-Market Ontology Interface",
  "description": "Stub interface defining GTM classes that connect to EFS ontology",
  "version": "0.1.0-interface",
  "status": "Interface Definition - Not Implemented",
  
  "gtm:interfaceClasses": [
    {
      "@id": "gtm:LaunchPlan",
      "@type": "Class",
      "name": "Launch Plan",
      "description": "Coordinated plan for market introduction",
      "rdfs:subClassOf": "schema:Plan",
      "gtm:inboundFromEFS": [
        {
          "sourceClass": "efs:Release",
          "relationship": "triggers",
          "cardinality": "one-to-one",
          "triggerCondition": "releaseType in ['MVP', 'Major', 'GA']"
        }
      ],
      "gtm:expectedProperties": [
        "gtm:launchDate",
        "gtm:launchType",
        "gtm:targetMarkets",
        "gtm:launchActivities",
        "gtm:successMetrics",
        "gtm:budget",
        "gtm:stakeholders"
      ]
    },
    {
      "@id": "gtm:ProductMessage",
      "@type": "Class",
      "name": "Product Message",
      "description": "Market-facing communication about product capabilities",
      "rdfs:subClassOf": "schema:Message",
      "gtm:inboundFromEFS": [
        {
          "sourceClass": "efs:Feature",
          "relationship": "supports",
          "cardinality": "many-to-many"
        }
      ],
      "gtm:expectedProperties": [
        "gtm:messageType",
        "gtm:headline",
        "gtm:valueStatement",
        "gtm:targetAudience",
        "gtm:proofPoints",
        "gtm:channels"
      ]
    },
    {
      "@id": "gtm:DifferentiationPoint",
      "@type": "Class",
      "name": "Differentiation Point",
      "description": "Unique competitive advantage in market positioning",
      "rdfs:subClassOf": "schema:PropertyValue",
      "gtm:inboundFromEFS": [
        {
          "sourceClass": "efs:Capability",
          "relationship": "enables",
          "cardinality": "many-to-many"
        }
      ],
      "gtm:expectedProperties": [
        "gtm:differentiatorType",
        "gtm:statement",
        "gtm:evidenceSupport",
        "gtm:competitorComparison",
        "gtm:sustainabilityScore"
      ]
    },
    {
      "@id": "gtm:MarketEntry",
      "@type": "Class",
      "name": "Market Entry",
      "description": "Strategy for entering a specific market segment",
      "rdfs:subClassOf": "schema:Action",
      "gtm:inboundFromEFS": [
        {
          "sourceClass": "efs:Release",
          "relationship": "supports",
          "cardinality": "many-to-many"
        }
      ],
      "gtm:expectedProperties": [
        "gtm:targetMarket",
        "gtm:entryStrategy",
        "gtm:timeline",
        "gtm:resources",
        "gtm:competitors",
        "gtm:barriers"
      ]
    }
  ],
  
  "gtm:suggestedExpansion": {
    "description": "Recommended additional classes for full GTM ontology",
    "classes": [
      "gtm:MarketingCampaign",
      "gtm:SalesEnablement",
      "gtm:PricingStrategy",
      "gtm:ChannelStrategy",
      "gtm:PartnerProgram",
      "gtm:CompetitorIntelligence",
      "gtm:MarketResearch",
      "gtm:BrandGuideline",
      "gtm:ContentAsset",
      "gtm:DemandGeneration",
      "gtm:LeadQualification",
      "gtm:SalesProcess",
      "gtm:CustomerOnboarding"
    ]
  },
  
  "gtm:launchReadinessFramework": {
    "description": "GTM readiness checklist triggered by EFS Release",
    "categories": [
      {
        "@id": "gtm:ProductReadiness",
        "name": "Product Readiness",
        "efsInput": "efs:Release.includedFeatures",
        "checkpoints": [
          "Feature completeness verified",
          "Quality gates passed",
          "Documentation complete",
          "Support training delivered"
        ]
      },
      {
        "@id": "gtm:MarketingReadiness",
        "name": "Marketing Readiness",
        "efsInput": "efs:Feature → gtm:ProductMessage",
        "checkpoints": [
          "Messaging approved",
          "Collateral created",
          "Website updated",
          "PR prepared"
        ]
      },
      {
        "@id": "gtm:SalesReadiness",
        "name": "Sales Readiness",
        "efsInput": "efs:Capability → gtm:DifferentiationPoint",
        "checkpoints": [
          "Pricing finalized",
          "Sales training complete",
          "Demo environment ready",
          "Objection handling documented"
        ]
      },
      {
        "@id": "gtm:OperationalReadiness",
        "name": "Operational Readiness",
        "efsInput": "efs:Release.releaseDate",
        "checkpoints": [
          "Support processes defined",
          "SLAs established",
          "Monitoring in place",
          "Escalation paths clear"
        ]
      }
    ]
  },
  
  "gtm:releaseToLaunchMapping": {
    "description": "Mapping between EFS Release types and GTM Launch types",
    "mappings": [
      {
        "efsReleaseType": "efs:MVP",
        "gtmLaunchType": "gtm:SoftLaunch",
        "activities": ["Limited beta", "Early adopter program", "Feedback collection"]
      },
      {
        "efsReleaseType": "efs:Beta",
        "gtmLaunchType": "gtm:BetaProgram",
        "activities": ["Public beta", "Community building", "Bug bounty"]
      },
      {
        "efsReleaseType": "efs:Major",
        "gtmLaunchType": "gtm:MajorLaunch",
        "activities": ["Press release", "Launch event", "Partner announcements"]
      },
      {
        "efsReleaseType": "efs:GA",
        "gtmLaunchType": "gtm:GeneralAvailability",
        "activities": ["Full marketing campaign", "Sales enablement", "Customer migration"]
      },
      {
        "efsReleaseType": "efs:Minor",
        "gtmLaunchType": "gtm:FeatureAnnouncement",
        "activities": ["Blog post", "Email announcement", "In-app notification"]
      },
      {
        "efsReleaseType": "efs:Patch",
        "gtmLaunchType": "gtm:SilentRelease",
        "activities": ["Release notes", "Support KB update"]
      }
    ]
  }
}
