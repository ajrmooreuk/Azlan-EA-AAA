  "integrations": {
    "baivProcessIntegration": {
      "@id": "vsom:BAIVProcessIntegration",
      "description": "Integration with BAIV P1-P7 Process Framework",
      "phases": {
        "P1Configuration": {
          "vsomInput": ["MarketingBudget", "BrandValues", "VisionStatement"],
          "output": ["SystemReady", "BaselineConfiguration"]
        },
        "P2Discovery": {
          "vsomInput": ["CurrentState", "CompetitorAnalysis", "MarketingStrategy"],
          "output": ["DigitalFootprint", "CompetitiveLandscape"]
        },
        "P3Capture": {
          "vsomInput": ["BaselineMetrics", "PerformanceTargets"],
          "output": ["PerformanceData", "MetricBaselines"]
        },
        "P4Audit": {
          "vsomInput": ["CurrentPerformance", "StrategyAlignment"],
          "output": ["VisibilityScore", "GapIdentification"]
        },
        "P5Analytics": {
          "vsomInput": ["KPIBaselines", "ObjectiveTargets"],
          "output": ["OpportunityMatrix", "PerformanceInsights"]
        },
        "P6GapAnalysis": {
          "vsomInput": ["ObjectivesVsCurrent", "TargetGaps"],
          "output": ["GapReport", "PriorityMatrix"]
        },
        "P7Ideation": {
          "vsomInput": ["AllVSOMElements", "GapAnalysis"],
          "output": ["30DayActionPlan", "ImplementationRoadmap"]
        }
      }
    },
    "raciIntegration": {
      "@id": "vsom:RACIIntegration",
